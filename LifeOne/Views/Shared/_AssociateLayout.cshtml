        /*------- Preloader JS Start ------*/
        $(window).on("load", function () {
            $("#preloader").fadeOut();
            $("#loader-area").fadeOut(9000);
        })
        var referrer;
        $(document).ready(function () {
            referrer = document.referrer;
        });
        function BackPage() {
            location.href = referrer
        }
