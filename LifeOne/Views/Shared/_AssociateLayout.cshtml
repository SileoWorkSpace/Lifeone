    <style>
        #preloader {
            position: absolute;
            top: 50%;
            left: 50%;
            width: 50px;
            height: 50px;
            margin: -30px 0 0 -30px;
        }

        #loader-area {
            position: fixed;
            z-index: 999999;
            top: 0;
            background-image: url('../../Content/assests/images/prettyPhoto/dark_rounded/loader.gif');
            right: 0;
            bottom: 0;
            left: 0;
            display: block;
            background: #fff;
            background-repeat: no-repeat;
            background-position: center;
        }

        .img-part {
            position: relative;
        }

        span.recognition {
            position: absolute;
            width: 24px;
            height: 24px;
            top: 10px;
            right: -10px;
            border-radius: 50%;
            overflow: hidden;
            z-index: 999;
            line-height: 12px;
            padding: 5px;
            display: block;
            background: #fff;
            animation: mirrorflip 2s 0s linear infinite;
        }

        .gold {
            width: 100%;
            text-align: center;
            display: inline-block;
            padding: 4px;
            background: #ffbe0b;
            border-radius: 18px;
            line-height: 1;
            margin: 0 auto;
        }

        @@keyframes mirrorflip {
            0% {
                transform: scale(1, 1);
                -webkit-transform: scale(1, 1);
                -moz-transform: scale(1, 1);
                -o-transform: scale(1, 1);
                -ms-transform: scale(1, 1);
            }

            50% {
                transform: scale(-1, 1);
                -webkit-transform: scale(-1, 1);
                -moz-transform: scale(-1, 1);
                -o-transform: scale(-1, 1);
                -ms-transform: scale(-1, 1);
            }

            100% {
                transform: scale(1, 1);
                -webkit-transform: scale(1, 1);
                -moz-transform: scale(1, 1);
                -o-transform: scale(1, 1);
                -ms-transform: scale(1, 1);
            }
        }
    </style>
    <style>
