        #preloader {
            position: absolute;
            top: 50%;
            left: 50%;
            width: 50px;
            height: 50px;
            margin: -30px 0 0 -30px;
        }

        #loader-area {
            position: fixed;
            z-index: 999999;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            display: block;
            background: #fff;
            background-image: url('../../Content/assests/images/prettyPhoto/dark_rounded/loader.gif');
            background-repeat: no-repeat;
            background-position: center;
        }

        .hide {
            display: none;
        }

        .modal-header {
            display: block;
        }

        .content-page {
            overflow: auto !important;
        }
        /* .table-responsive {
            overflow-y: auto;
            height: 502px;
        }*/
        table {
            overflow-y: auto;
            height: 0vh; /* !!!  HEIGHT MUST BE IN [ vh ] !!! */
        }

        thead th {
            position: sticky;
            top: 0;
        }

        .disabled-link {
            pointer-events: none;
            cursor: default;
            color: #ccc;
        }
