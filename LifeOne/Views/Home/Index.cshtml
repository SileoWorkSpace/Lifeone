                        str += '<li><i class="fas fa-star"></i></li>'
                    }
                    else if (response[0].AvgRating == "2") {
                        str += '<li><i class="fas fa-star"></i></li> <li><i class="fas fa-star"></i></li>'
                    }
                    else if (response[0].AvgRating == "3") {
                        str += '<li><i class="fas fa-star"></i></li> <li><i class="fas fa-star"></i></li> <li><i class="fas fa-star"></i></li>'
                    }
                    else if (response[0].AvgRating == "4") {
                        str += '<li><i class="fas fa-star"></i></li> <li><i class="fas fa-star"></i></li> <li><i class="fas fa-star"></i></li> <li><i class="fas fa-star"></i></li>'
                    }
                    else if (response[0].AvgRating == "5") {
                        str += '<li><i class="fas fa-star"></i></li> <li><i class="fas fa-star"></i></li> <li><i class="fas fa-star"></i></li> <li><i class="fas fa-star"></i></li> <li><i class="fas fa-star"></i></li>'
                    }
                    $("#productAvgstr").html(str)
                }
            }, error: function () {
                //alert('Some Thing wrong!');
                $("#pre-loader").hide();
            }
        });
    }
    function fAddToCartLife(Add, productId) {
        var Add = Add;
        var productId = productId;
        window.location.href = '/Home/Products?ProductId=' + productId + '&Add=' + Add
    }
