    $(document).ready(function () {
        debugger;
        var load = $('#MaritalStatusID').val();
        if (load == "Married") {
            $('#Married').prop("checked", true);
            $('#Div1').show();
        }
        else if (load == "Single") {
            $('#Single').prop("checked", true);
            $('#Div1').hide();
            $('#MarriageAnniversaryID').val('');
        }
   // Gender
        var gen = $('#GenderIG').val();
        if (gen == "Male") {
            $('#Male').prop("checked", true);
        }
        else if (gen == "Female") {
            $('#Female').prop("checked", true);
        }
       
    });
    function GetMarried() {
        debugger;
        var status = $('input[name=MaritalStatus]:checked').val();
        if (status == "Married") {
            $('#Div1').show();
        }
        else if (status == "Single") {
            $('#Div1').hide();
            $('#MarriageAnniversaryID').val('');
        }
        else {
            $('#Div1').hide();
        }
    }
     var ArrFamilyDetails = [];
   

     function ShowAllImage(path) {
            $("#allImage").attr('src',   path.replace("~", ""));
