@model LifeOne.Models.AdminManagement.AEntity.MAdminVendorDetails

@{
    ViewBag.Title = "VendorOrderList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.22/pdfmake.min.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/0.4.1/html2canvas.min.js"></script>

<div class="container-fluid">
    <div class="row page-titles">
        <div class="ptitle">
            <h4 class="mb-1 mt-0">Vendor Orders</h4>
            <nav aria-label="breadcrumb" class="float-left">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#-">Home</a></li>
                    <li class="breadcrumb-item"><a href="#-">Reports</a></li>
                    <li class="breadcrumb-item active" aria-current="page">Vendor Orders Report</li>
                </ol>
            </nav>
        </div>
        <div class="pbutton">
            <div class="right text-right">
                <span>
                    <button type="button" class="btn btn-success btn-rounded btngreen"><i class="fa fa-file-excel-o"></i></button>
                </span>
            </div>
        </div>
    </div>
    <div id="no-more-tables">
        <div class="row">
            <div class="col-lg-12">
                <div class="box">
                    <div class="box-body">
                        <div class="row">
                            <div class="col-md-9">
                                <h4 class="header-title mt-0 mb-1">Vendor Reports</h4>
                            </div>
                            <div class="col-md-3 pull-right text-right" style="position:relative">

                                <a class="btn btn-outline-warning small advance-search" title="Click here for search" data-toggle="tooltip" data-placement="bottom">Search&nbsp;<i class="fa fa-search"></i></a>

                            </div>
                        </div>
                        <div class="row" id="advance-search" style="display:none;">
                            <div class="col-md-12">
                                <div class="alert alert-warning" style="color:#727272 !important">
                                    <div class="row">
                                        <div class="col-12 col-md-4">
                                            <div class="form-group">
                                                <label>Login ID</label>
                                                <input type="text" name="LoginId" id="LoginID" placeholder="" class="form-control">

                                            </div>
                                        </div>
                                        <div class="col-12 col-md-4">
                                            <div class="form-group">
                                                <label>Mobile</label>
                                                <input type="text" name="Mobile" id="Mobile" placeholder="" class="form-control">

                                            </div>
                                        </div>
                                        <div class="col-12 col-md-4">
                                            <div class="form-group">
                                                <label>Name</label>
                                                <input type="text" name="Name" id="Name" placeholder="" class="form-control">
                                            </div>
                                        </div>
                                        <div class="col-12 col-md-4">
                                            <div class="form-group">
                                                <label style="display:block;"></label>
                                                <input type="submit" class="btn btn-success " style="margin-top:1.5em;" value="Search" id="BtnSearch">
                                                <a href="/Admin/AdminReport/VendorOrderList" class="btn btn-danger" style="margin-top:1.5em;">Reset</a>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="table-responsive" >
                            <table class="table table-striped mb-0">
                                <thead>
                                    <tr>
                                        <th>#Sn.</th>
                                        <th>Login Id</th>
                                        <th>Name</th>
                                        <th>Mobile</th>
                                        <th>Product Name</th>
                                        <th>Product Type</th>
                                        <th>Order Number</th>
                                        <th>Order Date</th>
                                        <th>Order Amount</th>
                                        <th>Transaction Id</th>


                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model != null && Model.Objlist.Count > 0)
                                    {
                                        int SNo = 0;
                                        foreach (var item in Model.Objlist)
                                        {

                                            <tr>
                                                <td data-title="#SN:">@(SNo = SNo + 1)</td>
                                                <td data-title="Name:">@item.LoginId</td>

                                                <td data-title="Name:">@item.Name</td>
                                                <td data-title="Mobile:">@item.Mobile</td>
                                                <td data-title="Email:">@item.ProductName</td>
                                                <td data-title="Email:">@item.Producttype</td>
                                                <td data-title="Email:">@item.OrderNo</td>
                                                <td data-title="Email:">@item.CreatedDate</td>
                                                <td data-title="Email:">@item.TotalAmount</td>
                                                <td data-title="Email:">@item.TransactionNo</td>
                                                @*<td data-title="Number:">
                                                        <a href="Admin-GetDetailedVendorList/{{transac.Fk_VendorId}}">View Ledger</a>
                                                    </td>*@

                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>

                        </div>

                        <!-- /.box-body -->
                    </div>
                    <!-- /.box -->

                </div>
                <!-- /.col -->
            </div>

        </div>
    </div>
</div>