@model LifeOne.Models.Reports


    @{
        ViewBag.Title = "PremiumClubQualifier";
        Layout = "~/Views/Shared/_AdminLayout.cshtml";
    }

    <style>
        .alert-warning {
            background-color: #fcf8e3 !important;
            border-color: #faebcc !important;
        }
    </style>

    <div class="content">
        <div class="content-wrapper-before"></div>
        <!-- Start Content-->
        <div class="container-fluid">
            <div class="row page-titles">
                <div class="ptitle">
                    <h4 class="mb-1 mt-0 ">Premimum Club Qualifier</h4>

                    @*<nav aria-label="breadcrumb" class="float-left mt-1">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="/Admin-Dashboard">Dashboard</a></li>
                            <li class="breadcrumb-item"><a href="#">Reports</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Premimum Club Qualifier</li>
                        </ol>
                    </nav>*@
                </div>

            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="card">
                        <div class="card-body p-0 pt-10" style="padding-top:15px !important;">

                            <div class="table-responsive" >
                                <table class="table table-striped mb-0" id="tblPremimumclubmemberList">
                                    <thead>
                                        <tr>
                                            <th>S.No.</th>
                                            <th>Login ID</th>
                                            <th>Direct Seller Name</th>
                                            <th>Mobile No</th>

                                            <th>DOJ</th>
                                            <th>DOA</th>
                                            <th>DOLS</th>
                                            <th>DOQ</th>
                                            <th>Completed</th>
                                            <th>No of Sponsoring</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model != null && Model.dtGetPremiumClubQualifier.Rows.Count > 0)
                                        {

                                            for (var i = 0; i < Model.dtGetPremiumClubQualifier.Rows.Count; i++)
                                            {
                                              
                                                <tr>
                                                    <td>@(i+1)</td>
                                                    <td >@Model.dtGetPremiumClubQualifier.Rows[i]["LoginId"].ToString()</td>
                                                    <td >@Model.dtGetPremiumClubQualifier.Rows[i]["DisplayName"].ToString()</td>
                                                    <td >@Model.dtGetPremiumClubQualifier.Rows[i]["Mobile"].ToString()</td>
                                                    <td >@Model.dtGetPremiumClubQualifier.Rows[i]["DOJ"].ToString()</td>
                                                    <td >@Model.dtGetPremiumClubQualifier.Rows[i]["DOA"].ToString()</td>
                                                    <td >@Model.dtGetPremiumClubQualifier.Rows[i]["DOLS"].ToString()</td>
                                                    <td >@Model.dtGetPremiumClubQualifier.Rows[i]["DOQ"].ToString()</td>
                                                    <td >@Model.dtGetPremiumClubQualifier.Rows[i]["AchievedSatus"].ToString()</td>
                                                    <td >@Model.dtGetPremiumClubQualifier.Rows[i]["NoofSp"].ToString()</td>


                                                </tr>
                                            }
                                        }

                                    </tbody>
                                </table>
                            </div>
                        </div>

                        <footer class="card-footer bg-white p-2">
                            <div class="d-md-flex align-items-center justify-content-between">



                            </div>
                        </footer>
                    </div>
                </div>
            </div>
        </div>
        <!-- container-fluid -->

    </div>

