@section body{
    <script type="text/javascript" src="https://cdn.jsdelivr.net/jquery/latest/jquery.min.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/momentjs/latest/moment.min.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js"></script>
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css" />
    <script>
        $(function () {
            $('input[name="ReqDate"]').daterangepicker({
                //opens: 'left'
                timePicker: true,
                startDate: moment().startOf('month'),
                endDate: moment().endOf('month'),
                locale: {
                    format: 'DD/M/YYYY'
                }
            }, function (start, end, label) {
                console.log("A new date selection was made: " + start.format('YYYY-MM-DD') + ' to ' + end.format('YYYY-MM-DD'));
            });
        });


        function invoiceDate(ReqId) {
            $('#hdnRequestId').val(ReqId);
            $('#InvoiceDate').val('');
        }
        function UpdateInvoiceDate() {
            var ReqId = $('#hdnRequestId').val();
            var InvoiceDate = $('#InvoiceDate').val();
            if (InvoiceDate == null || InvoiceDate == '') {
                alert("Please Select Date!!");
                return false;
            }
            var todayDate = new Date().toISOString().slice(0, 10);
            if (InvoiceDate > todayDate) {
                alert("Invoice date should not be greater than today's date");
                return false;
            }
            if (confirm("Are you sure want to update invoice date ?")) {
                $.ajax({
                    url: "../../Admin/ManageAssociateOrdered/UpdateInvoiceDate",
                    type: "POST",
                    dataType: 'json',
                    data: JSON.stringify({ ReqId: ReqId, InvoiceDate: InvoiceDate }),
                    contentType: "application/json",
                    success: function (result) {
                        if (result.Flag == 1) {
                            alert(result.Msg);
                            window.setTimeout(function () {
                                window.location.reload();
                            }, 2000);
                        }
                        else {
                            alert(result.Msg);
                            window.setTimeout(function () {
                                window.location.reload();
                            }, 2000);
                        }
                    },
                    error: function (err) {
                        showSnazzyErrorMessage1(err.statusText);
                        return false;
                    }
                });
            }
            else
                return false;
        }

    </script>
}
