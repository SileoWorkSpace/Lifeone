    function Assign_Employee() {

        var LoginId = $("#lID").text();
        var SupportRequestList = $("#SupportRequestList").val();
        var EmployeeId = $("#empList").val();
        if (EmployeeId == null || EmployeeId <= 0 || EmployeeId == undefined || EmployeeId == '') {
            $("#empList").focus();
            return false;
        }

        if (SupportRequestList == null || SupportRequestList.length <= 0 || SupportRequestList == undefined || SupportRequestList == '') {
            $("#SupportRequestList").focus();
            return false;
        }

        if (LoginId != null && LoginId != undefined && LoginId != '') {
            $.ajax({
                url: "/Admin/AdminReport/Assign_Employee",
                type: "POST",
                dataType: 'json',
                data: JSON.stringify({ LoginId: LoginId, EmployeeId: EmployeeId, SupportRequestList: SupportRequestList }),
                contentType: "application/json",
                async: false,
                success: function (result) {
                    debugger
                    alert(result.data.Remark);
                    $("#myModal").modal('hide');
                },
                error: function (err) {
                    alert(err.statusText);
                    return false;
                }
            });
        }

        else
            return false
    }
