@model LifeOne.Models.AdminManagement.AEntity.MAdminIncomeType

@{
    ViewBag.Title = "GetMagicMoveIdWise";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

    <section class="content-header">
        <div class="col-md-4">
            <h1> <small>User's Magic Move Income Details</small> </h1>
            <ol class="breadcrumb" style="clear:both;background:none; padding:0px;">
                <li><a href="#-">Home</a></li>
                <li><a href="#-">Admin</a></li>
                <li class="active">User's Magic Move Income Details</li>
            </ol>
        </div>

        <div class="col-md-4">
            <div class="pbutton">
                <div class="right text-right">
                    <span>
                        <!--<button type="button" class="btn btn-success btn-rounded btngreen"><i class="fa fa-file-excel-o"></i></button>-->
                        <!--Total Amount:{{TotalAmount}}-->
                    </span>
                </div>
            </div>
        </div>
    </section>
    <section class="content">
        <div id="no-more-tables">
            <form id="form1" method="post">

                <div class="row">
                    <div class="col-xs-12">
                        <div class="box">
                            <div class="box-body">
                                <table id="myTable" class="table small table-hover">
                                    <thead>
                                        <tr>
                                            <th>#Sn.</th>
                                            <th>Login</th>
                                            <th>Name</th>
                                            <th>Mobile</th>
                                            <th>Credit Amount</th>
                                            <th>Debit Amount</th>
                                            <th>Remark</th>
                                            <th>Commisson Date</th>
                                        </tr>
                                    </thead>
                                    @if (Model != null && Model.Objlist.Count > 0)
                                        {
                                            int SNo = 0;
                                            foreach (var item in Model.Objlist)
                                            {

                                    <tr>
                                        <td data-title="#SN:">@(SNo = SNo + 1)</td>
                                        <td data-title="Login:">@item.LoginId</td>
                                        <td data-title="Name:">@item.Name</td>
                                        <td data-title="Mobile:">@item.Mobile</td>
                                        <td data-title="Email:">@item.CrAmount</td>
                                        <td data-title="Email:">@item.DrAmount</td>
                                        <td data-title="Email:">@item.Remark</td>
                                        <td data-title="Email:">@item.CreatedDate/td>
                                      
                                    </tr>
                                            }
                                        }
                                </table>
                                <!--<dir-pagination-controls max-size="10"
                                                     direction-links="true"
                                                     boundary-links="true" class="pull-right">
                            </dir-pagination-controls>-->


                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>
                    <!-- /.col -->
                </div>

            </form>
        </div>
    </section>




