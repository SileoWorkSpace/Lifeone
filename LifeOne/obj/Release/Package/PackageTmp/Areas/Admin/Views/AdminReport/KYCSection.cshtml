<script src="~/Content/dist/js/sweetalert.min.js"></script>
<script>

    //function OpenModal(AdhaarFront, AdhaarBack, AdhaarNumber, LoginId, status) {
    function OpenModal(AdhaarFront, AdhaarBack, AdhaarNumber, LoginId, status, panstatus, IsPanCardReject, remark) {

        $("#myModal1").modal("show");
        $('#AdhaarFront').attr('src', AdhaarFront.replace('~/', '/'));
        $('#AdhaarBack').attr('src', AdhaarBack.replace('~/', '/'));
        $('#AdhaarNumber').val(AdhaarNumber);
        $('#hdnLoginId').val(LoginId);
        if ((panstatus == "") || (IsPanCardReject == "False" && panstatus == "False")) {
            $("#chkModl").css('display', '')
            $("#ModalbtnApprove").css('display', '')
            $('#ModalbtnApprove').attr('disabled', false);
            $('#ModalbtnDecline').css('display', 'none');
