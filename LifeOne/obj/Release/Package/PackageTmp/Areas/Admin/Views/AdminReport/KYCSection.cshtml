@model LifeOne.Models.AdminManagement.AEntity.MemberKYC
@{
    ViewBag.Title = "KYCSection";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<link href="~/Content/dist/css/sweetalert.css" rel="stylesheet" />
<div class="content">
    <!-- Start Content-->
    <div class="container-fluid">
        <div class="row page-titles">
            <div class="ptitle">
                <h4 class="mb-1 mt-0">KYC List</h4>
                <nav aria-label="breadcrumb" class="float-left mt-1">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="#">Dashboard</a></li>
                        <li class="breadcrumb-item"><a href="#">KYC</a></li>
                        <li class="breadcrumb-item active" aria-current="page">KYC List</li>
                    </ol>
                </nav>
            </div>
            <div class="pbutton">
                <div class="right text-right">
                    <span>
                        @*<a class="advance-search" title="Click here for search" data-toggle="tooltip" data-placement="bottom" href="#-">Search</a>*@
                    </span>
                    <span>
                        <a href="/Admin/AdminReport/ExportToExcelKYC" class="btn btn-success btn-rounded btngreen"><i class="fa fa-file-excel-o"></i></a>
                    </span>
                    <span>
                        <button type="button" class="btn btn-success btn-rounded btnPdf" id="btnpdf"><i class="fa fa-file-pdf-o"></i></button>
                    </span>

                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">

                        <div class="row" id="advance-search" style="display:none;">

                            <div class="col-md-12">
                                <div class="alert alert-warning" style="color:#727272 !important">

                                    <form action="" method="post">
                                        <div class="row">
                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label>Login ID</label>
                                                    <input type="text" name="LoginId" id="LoginID" placeholder="" class="form-control">

                                                </div>
                                            </div>
                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label>Mobile</label>
                                                    <input type="text" name="Mobile" id="Mobile" placeholder="" class="form-control">

                                                </div>
                                            </div>
                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label>Name</label>
                                                    <input type="text" name="Name" id="Name" placeholder="" class="form-control">

                                                </div>
                                            </div>
                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label>Status</label>
                                                    <select class="form-control" name="Status">
                                                        <option>Select</option>
                                                        <option value="Pending">Pending</option>
                                                        <option value="Approved">Approved</option>
                                                        <option value="Rejected">Rejected</option>
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label style="display:block;"></label>
                                                    <input type="submit" class="btn btn-success " style="margin-top:1.5em;" value="Search" id="BtnSearch">
                                                    <a href="/KYCSection" class="btn btn-secondary" style="margin-top:1.5em;">Reset</a>

                                                </div>
                                            </div>

                                        </div>
                                    </form>
                                </div>
                            </div>


                        </div>
                        <div class="table-responsive" >
                            <table class="table mb-0" id="myKYCList">
                                <thead>
                                    <tr>
                                        <th>S. No.</th>
                                        <th>Name</th>
                                        <th>LoginId</th>
                                        <th>Mobile</th>
                                        <th style="text-align:center">Address Front</th>
                                        <th style="text-align:center">Address Back</th>
                                        <th style="text-align:center">Pan card No.</th>
                                        <th style="text-align:center">Bank </th>
                                        <th>Updated Date</th>
                                        <th>Status</th>
                                        @*<th class="text-right"><b>View</th>*@
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model != null)
                                    {
                                        if (Model.List != null)
                                        {
                                            int sno = 0;
                                            foreach (var item in Model.List)
                                            {
                                                <tr>
                                                    <td align="left">@(sno = sno + 1)</td>
                                                    <td align="left"><span>@item.Name</span></td>
                                                    <td align="left"><span>@item.LoginId</span></td>
                                                    <td align="left"><span>@item.Mobile</span></td>
                                                    <td align="center">
                                                        <span class="mr-10">
                                                            <a onclick="OpenAddressModal('@item.AddressFrontUrl',  '@item.LoginId', '@item.Status', '@Html.Raw(item.IsAadharApproved)','@Html.Raw(item.IsAadharReject)','@item.AddressProofRejectRemark','@item.AdharCardNumber')"><i class="fa fa-eye" aria-hidden="true"></i></a>
                                                        </span>

                                                        @if (item.IsAadharApproved == true)
                                                        {
                                                            <label class="badge badge-success">Approved</label>
                                                        }
                                                        else if (item.IsAadharReject == true)
                                                        {
                                                            <label class="badge badge-danger">Rejected</label>
                                                        }
                                                        else if (item.IsAadharReject == false && item.IsAadharApproved == false)
                                                        {
                                                            <label class="badge badge-warning">Pending</label>
                                                        }
                                                    </td>

                                                    <td align="center">
                                                        <span class="mr-10">
                                                            <a onclick="OpenAddressBackModal('@item.AddressBackUrl', '@item.LoginId', '@item.Status', '@Html.Raw(item.IsApproveAddressBackProof)','@Html.Raw(item.IsAddressBackProofReject)','@item.AddressProofBackRejectRemark')"><i class="fa fa-eye" aria-hidden="true"></i></a>
                                                        </span>

                                                        @if (item.IsApproveAddressBackProof == "True")
                                                        {
                                                            <label class="badge badge-success">Approved</label>
                                                        }
                                                        else if (item.IsAddressBackProofReject == "True")
                                                        {
                                                            <label class="badge badge-danger">Rejected</label>
                                                        }
                                                        else if (item.IsAddressBackProofReject == "False" && item.IsApproveAddressBackProof == "False")
                                                        {
                                                            <label class="badge badge-warning">Pending</label>
                                                        }
                                                    </td>

                                                    <td align="center">
                                                        <span class="mr-10">
                                                            <a onclick="OpenModalPanCard('@item.PanCardURL', '@item.PanCard', '@item.LoginId', '@item.Status', '@Html.Raw(item.IsPanApproved)','@Html.Raw(item.IsPanCardReject)','@item.PanCardRejectRemark')"><i class="fa fa-eye" aria-hidden="true"></i></a>
                                                        </span>

                                                        @if (item.IsPanApproved == true)
                                                        {
                                                            <label class="badge badge-success">Approved</label>
                                                        }
                                                        else if (item.IsPanCardReject == true)
                                                        {
                                                            <label class="badge badge-danger">Rejected</label>
                                                        }
                                                        else if (item.IsPanApproved == false && item.IsPanCardReject == false)
                                                        {
                                                            <label class="badge badge-warning">Pending</label>
                                                        }
                                                    </td>
                                                    <td align="center">
                                                        <span class="mr-10">

                                                            <a onclick="OpenModalBank('@item.BankDocumentURL','@item.LoginId','@item.Status', '@Html.Raw(item.IsBankApproved)','@Html.Raw(item.IsBankReject)','@item.BankRejectRemark','@item.MemberAccNo')"><i class="fa fa-eye" aria-hidden="true"></i></a>
                                                        </span>

                                                        @if (item.IsBankApproved == true)
                                                        {
                                                            <label class="badge badge-success">Approved</label>
                                                        }
                                                        else if (item.IsBankReject == true)
                                                        {
                                                            <label class="badge badge-danger">Rejected</label>
                                                        }
                                                        else if (item.IsBankApproved == false && item.IsBankReject == false)
                                                        {
                                                            <label class="badge badge-warning">Pending</label>
                                                        }
                                                    </td>
                                                    <td align="left"><span>@item.date</span></td>
                                                    <td align="right">
                                                        @if (item.Status == "Pending")
                                                        {
                                                            <label class="badge badge-warning">@item.Status</label>
                                                        }
                                                        else if (item.Status == "Rejected")
                                                        {
                                                            <label class="badge badge-danger">@item.Status</label>
                                                        }
                                                        else if (item.Status == "Approved")
                                                        {
                                                            <label class="badge badge-success">@item.Status</label>
                                                        }
                                                        <input type="hidden" id="hdnLoginId" value="@item.LoginId" />
                                                        <input type="hidden" id="hdnStatus" value="@item.Status" />
                                                    </td>


                                                </tr>

                                            }
                                        }
                                    }

                                </tbody>
                            </table>

                        </div>
                    </div>

                    <footer class="card-footer bg-white p-2">
                        <div class="d-md-flex align-items-center justify-content-between">

                            @if (Model != null)
                            {
                                if (Model.Pager != null)
                                {
                                    if (Model.Pager.EndPage > 1)
                                    {
                                        <div> <small class="text-muted inline">showing @Model.List.Count  of @Model.List[0].TotalRecords items</small> </div>
                                        <div>
                                            <ul class="pagination pagination-sm m-t-none m-b-none">
                                                @if (Model.Pager.CurrentPage > 1)
                                                {
                                                    <li class="page-item">
                                                        <a class="page-link" href="/Admin/AdminReport/KYCSection?Name=@Model.Name&LoginId=@Model.LoginId&Mobile=@Model.Mobile">First</a>
                                                    </li>
                                                    <li class="page-item">
                                                        <a class="page-link" href="/Admin/AdminReport/KYCSection?page=@(Model.Pager.CurrentPage - 1)&Name=@Model.Name&LoginId=@Model.LoginId&Mobile=@Model.Mobile">Previous</a>
                                                    </li>
                                                }
                                                @for (var page = Model.Pager.StartPage; page <= Model.Pager.EndPage; page++)
                                                {
                                                    <li class="page-item @(page == Model.Pager.CurrentPage ? "active" : "")">
                                                        <a class="page-link" href="/Admin/AdminReport/KYCSection?page=@page&Name=@Model.Name&LoginId=@Model.LoginId&Mobile=@Model.Mobile">@page</a>
                                                    </li>
                                                }
                                                @if (Model.Pager.CurrentPage < Model.Pager.TotalPages)
                                                {
                                                    <li class="page-item">
                                                        <a class="page-link" href="/Admin/AdminReport/KYCSection?page=@(Model.Pager.CurrentPage + 1)&Name=@Model.Name&LoginId=@Model.LoginId&Mobile=@Model.Mobile">Next</a>
                                                    </li>
                                                    <li class="page-item">
                                                        <a class="page-link" href="/Admin/AdminReport/KYCSection?page=@(Model.Pager.TotalPages)&Name=@Model.Name&LoginId=@Model.LoginId&Mobile=@Model.Mobile">Last</a>
                                                    </li>
                                                }
                                            </ul>
                                        </div>
                                    }
                                }

                            }

                        </div>
                    </footer>
                </div>
            </div>
        </div>

    </div>
    <!-- container-fluid -->

</div>

