@model LifeOne.Models.AdminManagement.AEntity.MembersResponse
@{
    ViewBag.Title = "NotifyMembers";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<style>

    .alert-warning {
        background-color: #fcf8e3 !important;
        border-color: #faebcc !important;
    }

    .other-btns {
        position: absolute;
        right: 0;
        top: 0;
        z-index: 9;
        padding: 7px 13px;
    }

    .openCCBox, .openBCCBox {
        cursor: pointer;
    }

    .btn.btn-file {
        position: relative;
        overflow: hidden;
    }

    .fixedEmail {
        position: fixed;
        bottom: -0px;
        right: 0px;
        width: 500px;
        max-height: 485px;
        display: none;
        z-index: 999;
    }

        .fixedEmail .input-group .input-group-addon {
            background-color: #edf1f2;
            border-color: #cfdadd;
        }

        .fixedEmail textarea.form-control.to-box {
            border: 1px solid #cfdadd;
            padding: 0 5px;
            height: 60px;
            font-size: 12px;
        }

        .fixedEmail .field-icon, .FixedSMS .field-icon {
            position: absolute;
            top: 7px;
            right: 5px;
            z-index: 2;
        }

        .fixedEmail .form-group {
            margin-bottom: 3px;
            position: relative;
        }

        .fixedEmail .form-control, .fixedEmail .select2-container--default .select2-selection--multiple {
            border-color: #cfdadd;
            border-radius: 0;
            padding: 0 5px;
            font-size: 12px;
            height: 28px;
        }

        .fixedEmail .form-group.message-area {
            border: 1px solid #cfdadd;
        }

    .select2-container {
        width: 100% !important;
    }

    .openCCBox, .openBCCBox {
        cursor: pointer;
    }

    .CCBox, .BCCBox {
        display: none;
    }

    .fixedEmail textarea.form-control {
        padding: 5px;
        height: 150px;
        resize: none;
    }

    .fixedEmail ul.wysihtml5-toolbar a.btn {
        padding: 2px 5px;
        font-size: 12px;
    }

    .btn-custom {
        padding: 0 5px;
    }

    .custome-select {
        height: 25px;
        padding: 0 5px;
    }

    .btn.btn-file > input[type='file'] {
        position: absolute;
        top: 0;
        right: 0;
        min-width: 100%;
        min-height: 100%;
        font-size: 100px;
        text-align: right;
        opacity: 0;
        filter: alpha(opacity=0);
        outline: none;
        background: white;
        cursor: inherit;
        display: block;
    }

  
</style>

