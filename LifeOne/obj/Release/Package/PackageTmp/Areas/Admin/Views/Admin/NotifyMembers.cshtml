<div class="container-fluid">

    <form action="" method="post">
        <div class="row page-titles">
            <div class="ptitle">
                <h4 class="mb-1 mt-0">Notify Direct Sellers</h4>
                @*<nav aria-label="breadcrumb" class="float-left">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="/Admin-Dashboard">Dashboard</a></li>
                            <li class="breadcrumb-item"><a href="#-">Transactions</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Notify Members</li>
                        </ol>
                    </nav>*@
            </div>
            <div class="pbutton">
                <div class="right text-right">
                    <span>
                        @*<a class="advance-search" title="Click here for search" data-toggle="tooltip" data-placement="bottom" href="#-">Search</a>*@
                    </span>

                    <span>
                        <a class="btn btn-primary btn-sm text-white" onclick="Notification()">
                            <i class="fa fa-envelope"></i> Notification
                        </a>
                    </span>

                </div>
            </div>
        </div>
        <div id="no-more-tables">
            <div class="row">
                <div class="col-lg-12">
                    <div class="box">
                        <div class="box-body">
                            <div class="row" id="advance-search" style="display:none;">
                                <div class="col-md-12">
                                    <div class="alert alert-warning" style="color:#727272 !important">
                                        <div class="row">
                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label>Login Id</label>
                                                    <input type="text" name="LoginID" id="LoginID" placeholder="Enter Login Id" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label>Direct Seller</label>
                                                    <input type="text" name="MemberName" id="MemberName" placeholder="Enter Direct Seller" class="form-control">
                                                </div>
                                            </div>

                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label>Mobile No</label>
                                                    <input type="text" name="Mobile" id="Mobile" placeholder="Enter Mobile No" class="form-control" maxlength="10" onkeypress="return isNumberKey(event)" />
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-4">
                                                <div class="form-group">
                                                    <label style="display:block;"></label>
                                                    <input type="submit" class="btn btn-success " style="margin-top:1.5em;" value="Search" id="BtnSearch">
                                                    <a href="/Admin-NotifyMembers" class="btn btn-secondary" style="margin-top:1.5em;">Reset</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>

                            @*<button class="btn btn-info btn-sm" onclick="SMS()" disabled>
                                    <i class="fa fa-mobile"></i> SMS
                                </button>*@

                            @*<button class="btn btn-danger openEmail btn-sm" onclick="sendEmail()" disabled>
                                    <i class="fa fa-envelope"></i> Email
                                </button>*@
                            <div class="table-responsive" >
                                <table class="table table-striped mb-0" id="MyDownlineList">
                                    <thead>
                                        <tr>
                                            <th><input type="checkbox" id="chkParent" /></th>
                                            <th>#Sn.</th>
                                            <th>Login Id</th>
                                            <th>Direct Seller</th>
                                            <th>Sponsor's Id/Sponsor's Name</th>
                                            <th>Email Id</th>
                                            <th>State</th>
                                            <th>Join On</th>
                                            <th>Place Under Id</th>
                                            <th>Place Under Name</th>
                                            <th style="display:none"></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model != null && Model.List.Count > 0)
                                        {
                                           
                                            int i = 1;
                                            int page = Model.Pager.CurrentPage;
                                            page = (page - 1);
                                            i = ((page) * (Model.Pager.PageSize));
                                            foreach (var item in Model.List)
                                            {
                                                <tr>
                                                    <td><input type="checkbox" name="chk" class="chkchild" /></td>
                                                    <td data-title="#SN:">@(i=i + 1)</td>
                                                    <td data-title="Login:">@item.LoginId</td>
                                                    <td data-title="Name:">@item.MemberName</td>
                                                    <td data-title="Mobile:">@item.SponsorMobile <br /> @item.SponsorName</td>
                                                    <td data-title="Email:">@item.Email</td>
                                                    <td data-title="State:">@item.state</td>
                                                    <td data-title="Joining Date:">@item.joiningDate</td>
                                                    <td>@item.InviteCode</td>
                                                    <td>@item.InvitedBy </td>
                                                    <td style="display:none">@item.AndroidId </td>
                                                </tr>
                                            }
                                            i++;
                                        }
                                        else
                                        {
                                        }
                                    </tbody>
                                </table>

                            </div>
                        </div>

                        <footer class="card-footer bg-white p-2">
                            <div class="d-md-flex align-items-center justify-content-between">
                                @if (Model != null)
                                {
                                    if (Model.Pager != null)
                                    {
                                        if (Model.Pager.EndPage > 1)
                                        {
                                            <div> <small class="text-muted inline">showing @Model.List.Count  of @Model.List[0].ToatlRecord Direct Sellers</small> </div>
                                            <div>
                                                <ul class="pagination pagination-sm m-t-none m-b-none">
                                                    @if (Model.Pager.CurrentPage > 1)
                                                    {
                                                        <li class="page-item">
                                                            <a class="page-link" href="/Admin-NotifyMembers?Searchby=@Model.Searchby&SearchParam=@Model.SearchParam">First</a>
                                                        </li>
                                                        <li class="page-item">
                                                            <a class="page-link" href="/Admin-NotifyMembers?page=@(Model.Pager.CurrentPage - 1)&Searchby=@Model.Searchby&SearchParam=@Model.SearchParam">Previous</a>
                                                        </li>
                                                    }
                                                    @for (var page = Model.Pager.StartPage; page <= Model.Pager.EndPage; page++)
                                                    {
                                                        <li class="page-item @(page == Model.Pager.CurrentPage ? "active" : "")">
                                                            <a class="page-link" href="/Admin-NotifyMembers?page=@page&Searchby=@Model.Searchby&SearchParam=@Model.SearchParam">@page</a>
                                                        </li>
                                                    }
                                                    @if (Model.Pager.CurrentPage < Model.Pager.TotalPages)
                                                    {
                                                        <li class="page-item">
                                                            <a class="page-link" href="/Admin-NotifyMembers?page=@(Model.Pager.CurrentPage + 1)&Searchby=@Model.Searchby&SearchParam=@Model.SearchParam">Next</a>
                                                        </li>
                                                        <li class="page-item">
                                                            <a class="page-link" href="/Admin-NotifyMembers?page=@(Model.Pager.TotalPages)&Searchby=@Model.Searchby&SearchParam=@Model.SearchParam">Last</a>
                                                        </li>
                                                    }
                                                </ul>
                                            </div>
                                        }
                                    }

                                }

                            </div>
                        </footer>
                    </div>
                </div>
            </div>
        </div>
        <!-- container-fluid -->
    </form>
</div>
