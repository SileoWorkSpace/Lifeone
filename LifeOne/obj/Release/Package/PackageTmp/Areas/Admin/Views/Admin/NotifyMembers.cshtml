<script src="~/Content/dist/js/sweetalert.min.js"></script>
<script>
    var Fk_MemId = [];
    var Email = [];
    var NameNotification = [];
    $('.js-example-basic-multiple').select2();
    $(".openCCBox").click(function () {
        $('.CCBox').toggle();
    });
    $(".openBCCBox").click(function () {
        $('.BCCBox').toggle();
    });
    //Email
    $(".minimize-modal").click(function () {
        $('.fixedEmail .card-body').toggle();
        $('.fixedEmail .card-footer').toggle();
    });
    $(".close-modal").click(function () {
        $('.fixedEmail').hide();
    });
    $('#chkParent').on('click', function () {
        if (this.checked) {
            $('.chkchild').each(function () {
                this.checked = true;
                var name = [];
                $.each($("input[name='chk']:checked"), function () {
                    var data = $(this).parents('tr:eq(0)');
                    name.push($(data).find('td:eq(3)').text());
                });
                $("#txtname").val(name);

            });
        } else {
            $('.chkchild').each(function () {
                this.checked = false;
            });
        }
    });
    $(document).on("click", ".chkchild", function () {
        if ($('.chkchild:checked').length == $('.chkchild').length) {
            var name = [];
            $.each($("input[name='chk']:checked"), function () {
                var data = $(this).parents('tr:eq(0)');
                name.push($(data).find('td:eq(3)').text());
            });
            $("#txtname").val(name);
            $("#MemberId").val(MemberId);
            $('#chkParent').prop('checked', true);
        } else {
            var name = [];
            $.each($("input[name='chk']:checked"), function () {
                var data = $(this).parents('tr:eq(0)');
                name.push($(data).find('td:eq(3)').text());
            });
            $("#txtname").val(name);
            $('#chkParent').prop('checked', false);
        }
    });
    function Notification() {
        debugger
        NameNotification = [];
        var Count = 0;
        if ($("input[name='chk']:checked").val() != undefined) {
            $.each($("input[name='chk']:checked"), function () {
                var data = $(this).parents('tr:eq(0)');
                NameNotification.push($(data).find('td:eq(3)').text());
                Count += 1;
            });
            $("#txtnameNotification").val(NameNotification);
            $('#Totalcount').text(Count);
            $('#sendNotification').modal('show');
        } else {
            swal("", "No record selected !!!.", "warning")
            return false;
        }
    }
    function SMS() {
        if ($("input[name='chk']:checked").val() != undefined) {
            $('#sendSMS').modal('show');
        } else {
            swal("", "No record selected !!!.", "warning")
            return false;
        }
    }
    function sendEmail() {
        Email = [];
        Fk_MemId = [];
        if ($("input[name='chk']:checked").val() != undefined) {
            $.each($("input[name='chk']:checked"), function () {
                var data = $(this).parents('tr:eq(0)');
                Email.push($(data).find('td:eq(10)').text());
                Fk_MemId.push($(data).find('td:eq(9)').text());
            });
            $("#txtEmail").val(Email);
            $("#txtMailCC").val(Email);
            $("#txtMailBCC").val(Email);
            $('.fixedEmail').show();
        }
        else {
            swal("", "No record selected !!!.", "warning");
            $('.fixedEmail').hide();
            return false;
        }
    }
    $("#SendSMS").click(function () {
        var MemberId = [];
        var message = $("#txtsmsnotification").val();
        $.each($("input[name='chk']:checked"), function () {
            var data = $(this).parents('tr:eq(0)');
            MemberId.push($(data).find('td:eq(9)').text());
        });
        var model = {
            SMSContent: message,
            LstMemberId: MemberId
        };
        if (message != "") {
            $.ajax({
                type: "POST",
                url: "/Associate/AssociateTeam/SaveSMS",
                data: JSON.stringify(model),
                contentType: 'application/json; charset=utf-8',
                dataType: 'json',
                success: function (data) {
                    if (data != null && data != "") {
                        if (data.Flag == '1') {
                            swal("", data.Msg, "success");
                            window.setTimeout(function () {
                                window.location.href = "/Associate/AssociateTeam/MyDownline";
                            }, 3000);
                        }
                        else {
                            swal("", data.Msg, "warning");
                            window.setTimeout(function () {
                                window.location.href = "/Associate/AssociateTeam/MyDownline";
                            }, 3000);
                        }
                    }
                },
                failure: function (data) {
                    swal("", "Something went wrong!!!", "error");
                    window.setTimeout(function () {
                        window.location.href = "/Associate/AssociateTeam/MyDownline";
                    }, 3000);
                },
                error: function (data) {
                    swal("", "Something went wrong!!!", "error");
                    window.setTimeout(function () {
                        window.location.href = "/Associate/AssociateTeam/MyDownline";
                    }, 3000);
                }
            });
        }
        else {
            swal("", "Please type some content", "warning");
            $("#txtsmsnotification").val('');

        }
    });
    $("#SendNotification").click(function () {
        var MemberId = [];
        var Notificationmessage = $("#txtnotification").val();
        var Title = $("#txtTitle").val();
        if ($("#txtTitle").val() == "") {
            swal("", "Please enter title", "warning");
            return false;
        }
        $.each($("input[name='chk']:checked"), function () {
            var data = $(this).parents('tr:eq(0)');
            MemberId.push($(data).find('td:eq(10)').text());
        });
        var model = {
            Notificationmessage: Notificationmessage,
            Title: Title,
            LstMemberId: MemberId
        };
        if (Notificationmessage != "") {
            if (confirm("Are you sure to continue to send Notification to " + $('#Totalcount').text() + " users ?")) {
                $.ajax({
                    type: "POST",
                    url: "/Admin/Admin/SaveNotification",
                    data: JSON.stringify(model),
                    contentType: 'application/json; charset=utf-8',
                    dataType: 'json',
                    success: function (data) {
                        if (data != null && data != "") {
                            if (data.Flag == 1) {
                                swal("", data.Msg, "success");
                                location.reload();
                            }
                            else {
                                swal("", data.Msg, "warning");
                                window.setTimeout(function () {
                                    window.location.href = "/Admin-NotifyMembers";
                                }, 3000);
                            }
                        }
                    },
                    failure: function (data) {
                        swal("", "Something went wrong!!!", "error");
                        window.setTimeout(function () {
                            window.location.href = "/Associate/AssociateTeam/MyDownline";
                        }, 3000);
                    },
                    error: function (data) {
                        swal("", "Something went wrong!!!", "error");
                        window.setTimeout(function () {
                            window.location.href = "/Associate/AssociateTeam/MyDownline";
                        }, 3000);
                    }
                });
            }

            else
                return false;
        }
        else {
            swal("", "Please enter Message", "warning");
        }

    });
    $("#SendEmail").click(function () {
        var EmailMessage = $("#compose-textarea").val();
        var Subject = $("#txtSubject").val();
        var filepath = $("#hdnfilepath").val();
        var Emailccc = [];
        $('#txtMailCC :selected').each(function (i, sel) {
            Emailccc.push($(sel).text());
        });
        //var Emailccc = $("#txtMailCC option:selected").text();

