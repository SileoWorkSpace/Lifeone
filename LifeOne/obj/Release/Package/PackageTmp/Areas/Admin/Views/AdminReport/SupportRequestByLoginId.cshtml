                                <div class="alert alert-warning" style="color:#727272 !important">
                                    @using (Html.BeginForm("SupportRequestByLoginId", "AdminReport", FormMethod.Post))
                                    {
                                        <div class="row">
                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label>Name</label>
                                                    @Html.TextBoxFor(Model => Model.Name, new { @class = "form-control", @maxlength = "10" })

                                                </div>
                                            </div>
                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label>Mobile</label>
                                                    @Html.TextBoxFor(Model => Model.Mobile, new { @class = "form-control", @maxlength = "10" })

                                                </div>
                                            </div>

                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label>Ticket No</label>
                                                    @Html.TextBoxFor(Model => Model.TicketNumber, new { @class = "form-control", @maxlength = "20" })

                                                </div>
                                            </div>

                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label style="display:block;"></label>
                                                    <input type="submit" class="btn btn-success " style="margin-top:1.5em;" value="Search" id="BtnSearch">

                                                </div>
                                            </div>

                                        </div>
                                    }
                                </div>
                            </div>


                        </div>
                        <div class="table-responsive" >
                            <table class="table table-striped mb-0">
                                <thead>
                                    <tr>
                                        <th><b>S. No.</b></th>
                                        <th><b>Subject</b></th>
                                        <th><b>Ticket Number</b></th>
                                        <th><b>Mobile</b></th>
                                        <th><b>Send On</b></th>
                                        <th><b>Reply Message</b></th>
                                        <th><b>Replied On</b></th>
                                        <th><b>Document</b></th>
                                        <th><b>Action</b></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model != null)
                                    {
                                        if (Model.SupportList != null)
                                        {
                                            int sno = 0;
                                            foreach (var item in Model.SupportList)
                                            {

                                                <tr>
                                                    <td align="left">@(sno = sno + 1)</td>
                                                    <td align="left"><span>@item.Subject</span></td>
                                                    <td align="left"><span>@item.TicketNumber</span></td>
                                                    <td align="left"><span>@item.Mobile</span></td>
                                                    <td align="left"><span>@item.Date</span></td>
                                                    <td align="left"><span>@item.ReplyMessage</span></td>
                                                    <td align="left"><span>@item.RepliedOn</span></td>
                                                    <td align="left"><span><a href="#" onclick="ShowAllImage('@item.ImageUrl','Document')"><i class="fa fa-eye"></i></a></span></td>
                                                    <td align="left">

                                                        @if (item.Status == "Pending")
                                                        {
                                                            <a href="#" onclick="FillDataInToModal(@item.id,'@item.Subject','@item.Name')"><i class="fa fa-reply"></i>Reply</a>
                                                        }
                                                        else
                                                        {
                                                            <label class="badge badge badge-success">Replied</label>
                                                        }

                                                    </td>

                                                </tr>
                                            }
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal" id="myModal">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Support Reply</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>

                 Modal body 
                <form enctype="multipart/form-data" method="post">
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-md-1"></div>
                            <div class="col-md-10">

                                <div class="form-group">
                                    <label for="PaymentDate">To</label>
                                    <input type="text" id="ToName" class="form-control" readonly="readonly" />
                                </div>
                                <div class="form-group">
                                    <label for="Transaction">Subject</label>
                                    <input type="text" id="Subject" class="form-control" readonly="readonly" />
                                </div>
                                <div class="form-group">
                                    <label for="Transaction">Message</label>
                                    <textarea class="form-control" id="ReplyMessage"></textarea>
                                    <input type="hidden" id="hfPk_Id" />
                                </div>
                                <div class="form-group">
                                    <label for="Slip">Upload File</label>
                                    <input type="file" class="form-control" id="Support" onchange="uploadFile('Support', this)" />
                                    <input type="hidden" id="Supportimagepath">
                                </div>
                            </div>
                            <div class="col-md-1"></div>
                        </div>
                    </div>
                </form>
                 Modal footer 
                <div class="modal-footer">
                    <input type="button" value="Send" class="btn btn-primary" onclick="Reply()" />
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
                </div>

            </div>
        </div>
    </div>

    <div id="myModal1" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="myModalLabel"></h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"> <span aria-hidden="true">&times;</span> </button>
                </div>
                <div class="modal-body"> <img class="img-fluid" id="allImage" src="" alt="image" />  </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
                </div>
            </div>
             /.modal-content 
        </div>
         /.modal-dialog 
    </div>

</div>

<script>

    function ShowAllImage(path, name) {
        $("#allImage").attr('src', path.replace("~", ""));
        $('#myModalLabel').text(name);
        $("#myModal1").modal('show');
    }
    function FillDataInToModal(id, Subject, Name) {
        $('#hfPk_Id').val(id);
        $('#ToName').val(Name);
        $('#Subject').val(Subject);
        $('#myModal').modal("show");
    }


    function uploadFile(documentType, fileobj) {
        var fileInput = fileobj;
        if (fileInput.files.length === 0) {
            return;
        }
        var file = fileInput.files[0];
        var formData = new FormData();
        formData.append("documentType", documentType);
        formData.append("file", file);
        $.ajax({
            url: "/api/Image/UploadKyc",
            data: formData,
            processData: false,
            type: 'POST',
            contentType: false,
            success: function (data) {
                if (data.response == "success") {

                    if (documentType == "Support") {
                        //$scope.Supportimage = response.data.filepath.replace('~', '');
                        $('#Supportimagepath').val(data.filepath);
                        alert(data.msg);
                    }
                    else {
                        alert(response.data.msg);
                    }
                }
            }
        });
    }


    function Reply() {
        var messg = $('#ReplyMessage').val();
        if (messg == '') {
            alert("Please enter Some Message");
            return false;
        }
        var Id = $('#hfPk_Id').val();
        var uploadedfilepath = $('#Supportimagepath').val();
        $.ajax({
            method: 'POST',
            url: '/AdminReport/SaveSupportReply',
            data: { Message: messg, SupporPk_id: Id, Path: uploadedfilepath },
            success: function (data) {
                if (data.response == "success") {
                    alert("Successfully Replied");
                    location.reload();
                }
                else {
                    alert("Error Occured While processing the data");
                }
            }

        });




        <div id="login">
            <h3 class="text-center text-white pt-5">Login form</h3>
            <div class="container">
                <div id="login-row" class="row justify-content-center align-items-center">
                    <div id="login-column" class="col-md-6">
                        <div id="login-box" class="col-md-12">
                            <form id="login-form" class="form" action="" method="post">
                                <h3 class="text-center text-info">Login</h3>

                                <div class="form-group">
                                    <label for="password" class="text-info">Password:</label><br>
                                        <input type="text" name="password" id="password" class="form-control">
                                        </div>
                                        <div class="form-group">
                                            <label for="remember-me" class="text-info"><span>Remember me</span> <span><input id="remember-me" name="remember-me" type="checkbox"></span></label><br>
                                                <input type="submit" name="submit" class="btn btn-info btn-md" value="submit">
                                                </div>
                                                <div id="register-link" class="text-right">
                                                    <a href="#" class="text-info">Register here</a>
                                                </div>
                                            </form>
                                        </div>
                                </div>
                        </div>
                    </div>
                </div>


    }
</script>
