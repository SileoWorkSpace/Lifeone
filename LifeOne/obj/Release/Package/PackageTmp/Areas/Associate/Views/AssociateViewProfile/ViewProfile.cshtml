<!-- sample modal content -->
<div id="myModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="myModalLabel">Aadhar Image</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"> <span aria-hidden="true">&times;</span> </button>
            </div>
            <div class="modal-body"> <img class="img-fluid" id="allImage" src="" alt="Aadhar image" onerror="this.src = '';this.src='/Content/NewAssests/images/no-image(1).png'" />  </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
            </div>
        </div>
        <!-- /.modal-content -->

    </div>
    <!-- /.modal-dialog -->
</div>

<script>

    function ShowAllImage(path) {
        debugger;
        if (path != "") {
            $("#allImage").attr('src', path.replace("~", ""));
        }
        else {
            $('#allImage').attr('src', '/Content/NewAssests/images/no-image(1).png');
        }
        $("#myModal").modal('show');
    }
</script>
