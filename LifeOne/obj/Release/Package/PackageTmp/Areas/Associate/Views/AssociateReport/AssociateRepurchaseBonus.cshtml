@model LifeOne.Models.AdminManagement.AEntity.MPayoutReport
@{
    ViewBag.Title = "PayoutReport";
    Layout = "~/Views/Shared/_AssociateLayout.cshtml";
}
<style>
    .alert-warning {
        background-color: #fcf8e3 !important;
        border-color: #faebcc !important;
    }

    .tableincome {
    }

        .tableincome td:first-child {
            background: #fbfbfb;
            max-width: 40%;
        }

        .tableincome td {
            font-weight: bold;
        }
</style>
<div class="content">
    <div class="content-wrapper-before"></div>
    <!-- Start Content-->
    <div class="container-fluid">
        <div class="row page-titles">
            <div class="ptitle">
                <h4 class="mb-1 mt-0 text-white">Repurchase Payout Income</h4>
                @*<nav aria-label="breadcrumb" class="float-left mt-1">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/Associate-Dashboard">Dashboard</a></li>
                        <li class="breadcrumb-item"><a href="#">Repurchase Income</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Repurchase Payout Income</li>
                    </ol>
                </nav>*@
            </div>
            <div class="pbutton">
                <div class="right text-right">
                    <span>  <a class="advance-search" title="Click here for search" data-toggle="tooltip" data-placement="bottom" href="#-">Search</a></span>

                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body p-0">

                        <div class="row" id="advance-search" style="display:none;">
                            <div class="col-md-12">
                                @using (Html.BeginForm("AssociateRepurchaseBonus", "AssociateReport", FormMethod.Post))
                                {
                                    <div class="alert alert-warning" style="color:#727272 !important">
                                        <div class="row">
                                            <div class="col-12 col-md-3">
                                                <div class="form-group">
                                                    <label>Payout No</label>
                                                    <input type="text" name="PayoutNo" id="PayoutNo" placeholder="" class="form-control">
                                                    @*@Html.DropDownListFor(m => m.PayoutNo, new SelectList(ViewBag.PayoutMaster, "Value", "Text"), new { @class = "form-control", @id = "PayoutNo" })*@
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-3">
                                                <div class="form-group">
                                                    <label>From Date</label>
                                                    <input type="date" name="FromDate" id="FromDate" placeholder="" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-3">
                                                <div class="form-group">
                                                    <label>To Date</label>
                                                    <input type="date" name="ToDate" id="ToDate" placeholder="" class="form-control ">
                                                </div>
                                            </div>

                                            <div class="col-12 col-md-3">
                                                <div class="form-group">
                                                    <label class="display">&nbsp;</label>
                                                    <input type="submit" class="btn btn-success" value="Search" id="BtnSearch">
                                                    <a href="/Associate/AssociateReport/AssociateRepurchaseBonus" class="btn btn-secondary">Reset</a>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                }
                            </div>
                        </div>
                        <div class="table-responsive" >
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th nowrap>S. No.</th>
                                        <th nowrap>Payout No</th>
                                        <th nowrap>R Bonus</th>
                                        <th nowrap>Hybrid</th>
                                        <th nowrap>S. Royalty</th>
                                        <th nowrap>House</th>
                                        <th nowrap>Director</th>
                                        <th nowrap>Chairman</th>
                                        <th nowrap>LAR</th>
                                        <th nowrap>SG Yearly Bonus</th>
                                        <th class="text-right">Gross</th>
                                        <th class="text-right">TDS</th>
                                        <th class="text-right">Processing</th>
                                        <th class="text-right">Net</th>
                                        <th>Date</th>
                                        <th class="text-right">Action</th>
                                    </tr>
                                </thead>
                                <tbody>

                                    @if (Model != null && Model.Objlist.Count > 0)
                                    {
                                        int SNo = 0;
                                        foreach (var item in Model.Objlist)
                                        {

                                            <tr>
                                                <td data-title="#SN:">@(SNo = SNo + 1)</td>
                                                <td data-title="PayoutNo:">@item.PayoutNo</td>
                                                <td class="text-right" data-title="Repurchase Bonus:">@item.RepurchaseBonus</td>
                                                <td class="text-right" data-title="Hybrid Matchning Bonus:">@item.HybridMatchingBonus</td>
                                                <td class="text-right" data-title="LDB Bonus:">@item.LDBRepurchase</td>
                                                <td class="text-right" data-title="Car Fund:">@item.HouseFund</td>
                                                <td class="text-right" data-title="House Fund:">@item.DirectorClub</td>
                                                <td class="text-right" data-title="Director Fund:">@item.ChairManClub</td>
                                                <td class="text-right" data-title="Director Fund:">@item.LARBonus</td>
                                                <td class="text-right" data-title="LAR Bonus:">@item.SGYearlyBonus</td>
                                                <td class="text-right" data-title="Gross Amount:">@item.GrossAmount</td>
                                                <td class="text-right" data-title="TDS Amount:">@item.TDSAmount</td>
                                                <td class="text-right" data-title="Processing Fee:">@item.ProcessingFee</td>
                                                <td class="text-right" data-title="Net Amount:">@item.NetAmount</td>
                                                <td data-title="Closing Date:">@item.ClosingDate</td>
                                                <td>
                                                    <a class="btn btn-primary btn-sm" href="/StatementDetails?FK_MemId=@LifeOne.Models.Common.UrlEncodingDecoding.Encode(item.FK_MemId.ToString())&PayoutNo=@LifeOne.Models.Common.UrlEncodingDecoding.Encode(item.PayoutNo.ToString())">Statement</a>

                                                </td>
                                            </tr>
                                        }
                                    }
                                    else
                                    {
                                        @*<tr>
                                                <td colspan="10">No Record Found..</td>
                                            </tr>*@
                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- container-fluid -->
    <div id="myModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="myModalLabel">Statement Details</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"> <span aria-hidden="true">&times;</span> </button>
                </div>
                <div class="modal-body">
                    <div class="table-responsive" style="max-height:600px;">
                        <table class="table" id="TblEmpList">
                            <thead>
                                <tr>
                                    <th>From ID</th>
                                    <th>Display Name</th>
                                    <th>Business Amount</th>
                                    <th>Commission Percentage</th>
                                    <th>Income Type</th>
                                    <th>Closing Date</th>
                                    <th>Amount</th>
                                </tr>
                            </thead>
                            <tbody id="tblData"></tbody>
                            <tfoot>
                                <tr>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td class="text-right"><b>Total</b></td>
                                    <td><b><span id="spnTotal"></span></b></td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
                @*<div class="modal-footer">
                        <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
                    </div>*@
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>

    <!-- container-fluid -->
    <div id="myModalIncm" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="myModalLabel">Income Details</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"> <span aria-hidden="true">&times;</span> </button>
                </div>
                <div class="modal-body">
                    <div class="table-responsive" style="max-height:600px;">
                        <table class="table tableincome" id="TblList">
                            <tbody id="tblDataIncome"></tbody>

                        </table>

                    </div>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
</div>
