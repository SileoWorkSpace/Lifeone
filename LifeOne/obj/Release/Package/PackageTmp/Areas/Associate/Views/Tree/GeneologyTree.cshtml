
@{
    ViewBag.Title = "GeneologyTree";
    Layout = "~/Views/Shared/_AssociateLayout.cshtml";
}



<script src="~/Content/TreeAssets/GenerateTree.js?2.1"></script>
<link rel="stylesheet" type="text/css" href="~/Content/TreeAssets/css/binrary-tree.css">
<link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i" rel="stylesheet">

<div class="content-wrapper-before"></div>
<div class="container-fluid">
    <div class="row page-titles">
        <div class="ptitle">
            <h4 class="mb-1 mt-0 text-white">Tree</h4>


        </div>

    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body p-0 pt-10" style="padding-top:15px !important;">
                    <div class="row">
                        <div class="col-lg-6 offset-lg-3 ">
                            <div class="d-flex align-items-center justify-content-center mb-3">
                                <a href="javascript:void(0);" onclick="BindRoot();" class="btn btn-secondary mr-2">Root</a>
                                <a href="javascript:void(0);" onclick="UpLine();" class="btn btn-primary mr-2">UP</a>
                                <div class="input-group" style="float:left">
                                    <input type="text" class="form-control" value="Search" id="txtsearchlogin" />
                                    <input type="hidden" id="txtParentId" />
                                    <button type="submit" class="btn btn-success mr-2" onclick="SearchLogin();" style="border-radius: 0 5px 5px 0px;"><i class="fa fa-search"></i></button>
                                    @*<button type="button" class="btn btn-secondary">Go Up</button>*@
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="tf-tree example" style="margin-top:50px">
                        <ul>
                            <li>
                                <span id="DivRoot"></span>
                                <ul>
                                    <!-- Left Side -->
                                    <li>
                                        <span id="divRootLeft"></span>
                                        <ul>
                                            <li>

                                                <span id="divRootLeftLeft"></span>
                                                <ul>
                                                    <li>
                                                        <span id="divRootLeftLeftLeft"></span>
                                                    </li>
                                                    <li>
                                                        <span id="divRootLeftLeftLeftRight"></span>
                                                    </li>
                                                </ul>
                                            </li>
                                            <li>
                                                <span id="divRootLeftRight"></span>
                                                <ul>
                                                    <li>
                                                        <span id="divRootLeftRightLeft"></span>
                                                    </li>
                                                    <li>
                                                        <span id="divRootLeftRightRight"></span>
                                                    </li>
                                                </ul>
                                            </li>
                                        </ul>
                                    </li>
                                    <!-- Left Side -->
                                    <!-- Right Side -->
                                    <li>
                                        <span id="DivRootRight"></span>
                                        <ul>
                                            <li>
                                                <span id="DivRootRightLeft"></span>
                                                <ul>
                                                    <li>
                                                        <span id="DivRootRightLeftLeft"></span>
                                                    </li>
                                                    <li>
                                                        <span id="DivRootRightLeftRight"></span>
                                                    </li>
                                                </ul>
                                            </li>
                                            <li>
                                                <span id="DivRootRightRight"></span>
                                                <ul>
                                                    <li>
                                                        <span id="DivRootRightRightLeft"></span>
                                                    </li>
                                                    <li>
                                                        <span id="DivRootRightRightRight1"></span>
                                                    </li>
                                                </ul>
                                            </li>
                                        </ul>
                                    </li>
                                    <!-- Right Side -->
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>
