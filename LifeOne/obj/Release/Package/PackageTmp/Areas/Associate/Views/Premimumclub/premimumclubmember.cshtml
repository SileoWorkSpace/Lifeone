
@using LifeOne.Models.AssociateManagement.AssociateEntity
@model LifeOne.Models.AssociateManagement.AssociateEntity.PremiumClubMembers
@{
    ViewBag.Title = "Premimum Club Member";
    Layout = "~/Views/Shared/_AssociateLayout.cshtml";
}

<style>
    .alert-warning {
        background-color: #fcf8e3 !important;
        border-color: #faebcc !important;
    }
</style>

<div class="content">
    <div class="content-wrapper-before"></div>
    <!-- Start Content-->
    <div class="container-fluid">
        <div class="row page-titles">
            <div class="ptitle">
                <h4 class="mb-1 mt-0 text-white">Premimum Club Member</h4>

                <nav aria-label="breadcrumb" class="float-left mt-1">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Premimum Club Member</li>
                    </ol>
                </nav>
            </div>

        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body p-0 pt-10" style="padding-top:15px !important;">

                        <div class="table-responsive" >
                            <table class="table table-striped mb-0" id="tblPremimumclubmemberList">
                                <thead>
                                    <tr>
                                        <th>S. No.</th>
                                        <th>Login Id</th>
                                        <th>Name</th>
                                        <th>Mobile No</th>

                                        <th>Joining Date</th>
                                        <th>Activation Date</th>
                                        <th>Package Name</th>
                                        <th>No of Sponsoring</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model != null)
                                    {
                                        if (ViewBag.PCMemberLst != null)
                                        {
                                              int SNo = 0;
                                            foreach (var pclub in (List<PremiumClubMembers>)ViewBag.PCMemberLst)
                                            {
                                    <tr>
                                        <td>@(SNo = SNo + 1)</td>
                                        <td>@pclub.LoginId</td>
                                        <td>@pclub.DisplayName</td>
                                        <td>@pclub.Mobile</td>
                                        <td>@pclub.DOJ</td>
                                        <td>@pclub.DOA</td>
                                        <td>@pclub.PackageName</td>
                                        <td>@pclub.NoofSp</td>
                                    </tr>
                                            }
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <footer class="card-footer bg-white p-2">
                        <div class="d-md-flex align-items-center justify-content-between">



                        </div>
                    </footer>
                </div>
            </div>
        </div>
    </div>
    <!-- container-fluid -->

</div>
