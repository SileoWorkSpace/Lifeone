@model LifeOne.Models.Reports
@{
    ViewBag.Title = "Activated-Members";
    Layout = "~/Views/Shared/_AssociateLayout.cshtml";
}
<style>
    .alert-warning {
        background-color: #fcf8e3 !important;
        border-color: #faebcc !important;
    }

    .tableincome {
    }

        .tableincome td:first-child {
            background: #fbfbfb;
            max-width: 40%;
        }

        .tableincome td {
            font-weight: bold;
        }
</style>
<div class="content">
    <div class="content-wrapper-before"></div>
    <!-- Start Content-->
    <div class="container-fluid">
        <div class="row page-titles">
            <div class="ptitle">
                <h4 class="mb-1 mt-0 text-white">Active Direct Seller</h4>
                @*<nav aria-label="breadcrumb" class="float-left mt-1">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/Associate-Dashboard">Dashboard</a></li>
                        <li class="breadcrumb-item"><a href="#">My Order</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Active Members</li>
                    </ol>
                </nav>*@
            </div>
            <div class="pbutton">
                <div class="right text-right">
                    <span>  <a class="advance-search" title="Click here for search" data-toggle="tooltip" data-placement="bottom" href="#-">Search</a></span>
                    <span>
                        @*<a href="/Associate/AssociateReport/PayoutReportExportExcel" class="btn btn-success btn-rounded btngreen"><i class="fa fa-file-excel-o"></i></a>*@
                    </span><span>
                        @*<button type="button" class="btn btn-success btn-rounded btnPdf"><i class="fa fa-file-pdf-o"></i></button>*@
                    </span>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body p-0">

                        <div class="row" id="advance-search" style="display:none;">
                            <div class="col-md-12">
                                @using (Html.BeginForm("GetActiveMembers", "AssociateReport", FormMethod.Post))
                                {
                                    <div class="alert alert-warning" style="color:#727272 !important">
                                        <div class="row">
                                          
                                            <div class="col-12 col-md-3">
                                                <div class="form-group">
                                                    <label>From Date</label>
                                                    <input type="date" name="FromDate" id="FromDate" placeholder="" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-3">
                                                <div class="form-group">
                                                    <label>To Date</label>
                                                    <input type="date" name="ToDate" id="ToDate" placeholder="" class="form-control ">
                                                </div>
                                            </div>

                                            <div class="col-12 col-md-3">
                                                <div class="form-group">
                                                    <label class="display">&nbsp;</label>
                                                    <input type="submit" class="btn btn-success" value="Search" id="BtnSearch">
                                                    <a href="/Associate/AssociateReport/GetActiveMembers" class="btn btn-secondary">Reset</a>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                }
                            </div>
                        </div>
                        <div class="table-responsive" >
                            <table class="table table-striped mb-0">
                                <thead>
                                    <tr>

                                        <th>Franchise Code</th>
                                        <th>Franchise Name</th>
                                        <th>Package</th>
                                        <th>Login Id</th>
                                        <th>DisplayName </th>
                                        <th>Conatct</th>
                                        <th style="text-align:right">Amount</th>
                                        <th style="text-align:right">BV</th>
                                        <th style="text-align:right">PaymentMode</th>
                                        <th style="text-align:right">Transaction No</th>
                                        <th style="text-align:right">ActiveDate</th>
                                        <th style="text-align:right">Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model != null)
                                    {
                                        if (Model.dtGetActiveMembers != null)
                                        {
                                            if (Model.dtGetActiveMembers.Rows.Count > 0)
                                            {

                                                for (int i = 0; i <= Model.dtGetActiveMembers.Rows.Count - 1; i++)
                                                {
                                                    <tr class="text-left">

                                                        <td>@Model.dtGetActiveMembers.Rows[i]["FranchiseCode"].ToString()</td>
                                                        <td>@Model.dtGetActiveMembers.Rows[i]["FranchiseName"].ToString()</td>
                                                        <td>@Model.dtGetActiveMembers.Rows[i]["PackageName"].ToString()</td>
                                                        <td>@Model.dtGetActiveMembers.Rows[i]["LoginId"].ToString()</td>
                                                        <td>@Model.dtGetActiveMembers.Rows[i]["DisplayName"].ToString()</td>
                                                        <td>@Model.dtGetActiveMembers.Rows[i]["MobileNo"].ToString()</td>
                                                        <td style="text-align:right">@Model.dtGetActiveMembers.Rows[i]["Amount"].ToString()</td>
                                                        <td style="text-align:right">@Model.dtGetActiveMembers.Rows[i]["BV"].ToString()</td>
                                                        <td style="text-align:right">@Model.dtGetActiveMembers.Rows[i]["PaymentMode"].ToString()</td>
                                                        <td style="text-align:right">@Model.dtGetActiveMembers.Rows[i]["DDChequeNo"].ToString()</td>
                                                        <td style="text-align:right">@Model.dtGetActiveMembers.Rows[i]["ActiveDate"].ToString()</td>
                                                        <td>
                                                            <a href="/Associate/AssociateReport/ActivatedCustomerTaxInvoice?Pk_TransId=@Model.dtGetActiveMembers.Rows[i]["Pk_TransId"]" target="_blank" class="btn-sm btn  btn-info " title="View Invoice"><i class="fa fa-file"></i></a>
                                                        </td>
                                                    </tr>

                                                }
                                            }

                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- container-fluid -->
</div>
