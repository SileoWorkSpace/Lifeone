@model LifeOne.Models.FranchiseManagement.FEntity.MFranchiseProfile

@{
    ViewBag.Title = "FranchiseProfile";
    Layout = "~/Views/Shared/_FranchiseLayout.cshtml";
}





<div class="container-fluid">

    <div class="row page-titles">
        <div class="ptitle">
            <h4 class="mb-1 mt-0">Profile</h4>
            <nav aria-label="breadcrumb" class="float-left">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#-">Home</a></li>
                    <li class="breadcrumb-item"><a href="#-">Franchise</a></li>
                    <li class="breadcrumb-item active" aria-current="page">Profile</li>
                </ol>
            </nav>
        </div>

        <div class="right">
            <a class="btn btn-danger right" href="/Franchise/FranchiseProfile/FranchiseMaster">Edit Profile</a>
        </div>


    </div>
    @if (Model != null && Model.objList.Count > 0)
    {
        foreach (var item in Model.objList)
        {


            <div id="no-more-tables">
                <div class="row">

                    <div class="col-xs-12">
                        <div class="box">

                            <div class="box-body">

                                <div class="pbutton">
                                    <div class="right text-right">

                                        <a class="btn btn-lg" title="Click here to edit profile" href="/Admin/FranchiseMaster/FranchiseMaster?id=@Model.objList[0].PKFranchiseRegistrationId">
                                            <i class="fa fa-edit"></i>
                                        </a>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-1"></div>
                                    <div class="col-md-10 mt-10">
                                        <form action="#" name="registration">

                                            <div class="row">
                                                <div class="col-md-6">

                                                    <div class="form-group">
                                                        <label>First Name <span class="text-danger">*</span></label>
                                                        <div>@item.PersonName</div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label>Last Name <span class="text-danger">*</span></label>
                                                        <div>@item.PersonName</div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label>Mobile <span class="text-danger">*</span></label>
                                                        <div>@item.Mobile</div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label>Email <span class="text-danger">*</span></label>
                                                        <div>@item.Email</div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label>Father Name <span class="text-danger">*</span></label>
                                                        <div>@item.FatherName</div>
                                                    </div>
                                                </div>
                                                <!--<div class="col-md-6">
                                                    <div class="form-group">
                                                        <label>Password</label>
                                                        <input type="password" name="" id="pass" class="input-text form-control" placeholder="Password (Minimum 6 characters)" data-ng-model="Registration.password" required onblur="validatePassword()" autocomplete="off">
                                                    </div>
                                                </div>-->
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label>D.O.B <span class="text-danger">*</span></label>
                                                        <div>@item.DOB</div>

                                                    </div>
                                                </div>
                                                <div class="col-md-12">
                                                    <span style="font-weight:bold">Current Address</span>
                                                    <hr />
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label>Pin Code <span class="text-danger">*</span></label>
                                                        <div>@item.Cpincode</div>

                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label>state <span class="text-danger">*</span></label>
                                                        <div>@item.Cstate</div>

                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label>District <span class="text-danger">*</span></label>
                                                        <div>@item.Cstate</div>

                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <div class="form-group">
                                                            <label>Address <span class="text-danger">*</span></label>
                                                            <div>@item.CAddress</div>

                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-md-12">
                                                    <span style="font-weight:bold"> Permanent Address </span>
                                                    <hr />
                                                </div>

                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label>Pin Code <span class="text-danger">*</span></label>
                                                        <div>@item.Ppincode</div>

                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label>state <span class="text-danger">*</span></label>
                                                        <div>@item.Pstate</div>

                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label>District <span class="text-danger">*</span></label>
                                                        <div>@item.Pstate</div>

                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <div class="form-group">
                                                            <label>Address <span class="text-danger">*</span></label>
                                                            <div>@item.PAddress</div>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </form>
                                    </div>
                                    <div class="col-md-1"></div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        }
    }
</div>



