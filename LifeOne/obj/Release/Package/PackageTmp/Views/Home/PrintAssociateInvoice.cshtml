        int SNo = 0;
    }
    <div>
        <div class="content">

            <div>
                <table style="border:0;width:100%">
                    <tr>
                        <td style="text-align:center;">
                            <strong><strong style="font-size: small;">Tax Invoice</strong></strong>
                        </td>

                    </tr>
                </table>
            </div>



            <div id="divcontent">
                <div class="row-fluid">

                    <div class="span12">

                        <table class="table" style="border-top: 0px;width:100%;" cellspacing="0" cellpadding="0">
                            <tbody>

                                <tr style="border-top: 0px;">
                                    <td style="padding: 10px 0px 10px 10px; width: 25% !important; border-top: 0px;">
                                        <img src="~/Content/WebsiteCss/assets/images/others/logo.png" alt="Saler Logo" style="width: 110px;">
                                    </td>
                                    <td style="text-align: left; padding-right: 10px; border-top: 0px;font-size:12px">

                                        <strong>
                                            LIFE ONE WILLNESS INDUSTRIES PVT. LTD.
                                        </strong>

                                        <br />
                                        6/164 Vineet khand, Station Road, Lucknow 226010
                                        <br>
                                        +91 522 351 6249&nbsp;|&nbsp;, info@lifeonewellness.com<br />
                                        http://lifeonewellness.com<br>

                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="row-fluid" style="display:flex">
                    <div class="span12" style="padding:0;flex:1;">
                        <table class="table" style="width: 100% !important; float: left;" cellspacing="0" cellpadding="0">
                            <tbody>
                                <tr>
                                    <th style="border-top: none;width:130px">GST Number</th>
                                    <td style="border-top: none; font-weight: 800;">:&nbsp;<label id="lblGSTCopy"></label>  09ABGCS1826E1ZE</td>
                                </tr>
                                <tr>
                                    <th style="width:130px">Invoice Serial Number</th>
                                    <td>:&nbsp;<span id="lblInvoiceNo">@Model.OrderDetails.OrderNo</span></td>

                                </tr>
                                <tr>
                                    <th style="width:130px">Invoice Date</th>
                                    <td>
                                        :&nbsp;<span id="lblInvDate">@Model.OrderDetails.InvoiceDate</span>
                                    </td>
                                </tr>


                            </tbody>
                        </table>
                        <table class="table" style="width: 100%;" cellspacing="0" cellpadding="0">
                            <tbody>
                                <tr>
                                    <th style="border-top: none;width:130px">Copy</th>
                                    <td style="border-top: none;">:&nbsp;<span id="lblDocNo" style="display: none;">NA</span><span contenteditable="">Original</span></td>
                                </tr>
                                <tr>
                                    <th style="width:130px">Transport Mode</th>
                                    <td>:&nbsp;<span id="lblTransportMode">NA</span></td>
                                </tr>
                                <tr>
                                    <th style="width:130px">Vehicle Number</th>
                                    <td>:&nbsp;<span id="lblVehicalNumber">NA</span></td>
                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="row-fluid" style="display:flex">
                    <div class="span12" style="padding:0;flex:1;border-right: 1px solid #000;">
                        <table class="table" style="width: 100% !important; float: left; border-right: 1px solid;" cellspacing="0" cellpadding="0">

                            <tbody>
                                <tr>
                                    <th style="border-top: none;  background: #f1f1f1;text-align: left;" colspan="2">
                                        Billing Address

                                    </th>
                                </tr>
                                <tr>
                                    <th style="border-top: 1px solid;">Name</th>
                                    <td style="border-top: 1px solid;">:&nbsp;<span id="rptBillTo_lblCustomerName_0">@Model.OrderDetails.Name </span></td>
                                </tr>
                                
                                <tr>
                                    <th>Address</th>
                                    <td>:&nbsp;<span id="rptBillTo_lblAddress_0">@Model.OrderDetails.Address</span></td>

                                </tr>
                                <tr>
                                    <th>State</th>
                                    <td>:&nbsp;<span id="rptBillTo_lblAddress_0">@Model.OrderDetails.State</span></td>

                                </tr>
                                <tr>
                                    <th>State Code</th>
                                    <td>:&nbsp;<span id="rptBillTo_lblStateCode">@Model.OrderDetails.StateCode</span></td>
                                </tr>



                            </tbody>
                        </table>
                    </div>
                    <div class="span12" style="padding:0;flex:1;margin:0">
                        <table class="table wt-mn" style="width: 100% !important;" cellspacing="0" cellpadding="0">
                            <tbody>
                                <tr>
                                    <th style="border-top: none;  background: #f1f1f1;text-align: left;" colspan="2">
                                        Shipping Address

                                    </th>
                                </tr>
                                <tr></tr>
                                <tr>
                                    <th style="border-top: 1px solid;">Name</th>
                                    <td style="border-top: 1px solid;">:&nbsp;<span id="rptShipTo_lblCustomerName2_0"> @Model.OrderDetails.Name</span></td>

                                </tr>
                                
                                <tr>
                                    <th>Address</th>

                                    <td>:&nbsp;<span id="rptShipTo_lblAddress2_0">@Model.OrderDetails.Address</span></td>
                                </tr>
                                <tr>
                                    <th>State</th>
                                    <td>:&nbsp;<span id="rptBillTo_lblAddress_0">@Model.OrderDetails.State</span></td>

                                </tr>
                                <tr>
                                    <th>State Code</th>
                                    <td>:&nbsp;<span id="rptBillTo_lblStateCode">@Model.OrderDetails.StateCode</span></td>
                                </tr>




                            </tbody>
                        </table>

                    </div>
                </div>

                <div class="row-fluid">

                    <div class="span12" style="">
                        <table class="table inventory" style="border-bottom: 1px solid ;" cellspacing="0" cellpadding="0">
                            <thead>
                                <tr style="  background: #f1f1f1;border-right: 1px solid #000;border-bottom: 1px solid #000;	">
                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000"><span>#.</span></th>
                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000;text-align:left"><span>Product Name</span></th>
                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000;text-align:left"><span>HSN/<br />SAC Code</span></th>

                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000"><span>Qty.</span></th>

                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000"><span>Rate(₹)</span></th>
                                    @*<th style="border-top: none; padding: 0px;border-bottom: 1px solid #000">
                                            <span>BV</span><br>
                                        </th>*@
                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000; border-right:0px;width:40%">
                                        <table style="width: 100%;" cellspacing="0" cellpadding="0">
                                            <tbody>
                                                <tr>

                                                    <th colspan="2" style="border-bottom:1px solid #000;"><span>SGST</span></th>
                                                    <th colspan="2" style="border-bottom:1px solid #000;"><span>CGST</span></th>
                                                    <th colspan="2" style="border-bottom:1px solid #000;"><span>IGST</span></th>
                                                </tr>
                                                <tr>

                                                    <th style="data-rel=" tooltips" title="Rate(%)"><small style="font-size:8px;">(%)</small></th>
                                                    <th><small style="font-size:8px;">Amt</small></th>
                                                    <th style="data-rel=" tooltips" title="Rate(%)"><small style="font-size:8px;">(%)</small></th>
                                                    <th><small style="font-size:8px;">Amt</small></th>
                                                    <th style="data-rel=" tooltips" title="Rate(%)"><small style="font-size:8px;">(%)</small></th>
                                                    <th><small style="font-size:8px;">Amt</small></th>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </th>
                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000"><small>Taxable <br> Amount(₹)</small></th>

                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000; border-right: none;"><span>SubTotal(₹)</span></th>
                                </tr>
                            </thead>

                            <tbody style="min-height:400px">

                                @foreach (var item in Model.ItemList)
                                {

                                    SNo = SNo + 1;
                                    TotalQty = TotalQty + Convert.ToDecimal(item.Qty);
                                    TotalBV = TotalBV + Convert.ToDecimal(item.BV);
                                    TotalRate = TotalRate + Convert.ToDecimal(item.MRP);
                                    TotalSGST = TotalSGST + Convert.ToDecimal(item.GSTAmount/2);
                                    TotalCGST = TotalCGST + Convert.ToDecimal(item.GSTAmount / 2);
                                    TotalIGST = TotalIGST + Convert.ToDecimal(item.GSTAmount);
                                    TotalSGSTPer = TotalSGSTPer + Convert.ToDecimal(item.GSTPer / 2);
                                    TotalIGSTPerc = TotalIGSTPerc + Convert.ToDecimal(item.GSTPer );
                                    TotalCGSTPerc = TotalCGSTPerc + Convert.ToDecimal(item.GSTPer / 2);
                                    TotalTaxable = TotalTaxable + Convert.ToDecimal(item.TaxableAmount);
                                    TotalSubTotal = TotalSubTotal + Convert.ToDecimal(item.DP);
                                    //TotalPV = TotalPV + item.PointValue;

                                    <tr>
                                        <td><span>@SNo</span></td>
                                        <td style="text-align: left;"><span class="product-name"> @item.ProductName</span></td>
                                        <td style="text-align: left;"><span class="product-name"> @item.HSNCode</span></td>


                                        <td style="text-align: center;"><span class="salqty"> @item.Qty</span></td>

                                        <td style="text-align: center;"><span class="prorate">@item.SingleDP</span></td>

                                        @*<td style="text-align: center;">
                                                <span class="bvline">
                                                    @item.PointValue
                                                </span>
                                            </td>*@

                                        <td style="padding: 0px; border-right:0px">
                                            <table style="width: 100%;" cellspacing="0" cellpadding="0" border="0">
                                                <tbody>
                                                    @*<tr>
                                                            <td style="width:15%; padding:2px;text-align:right;font-size:10px;"> @item.SGST</td>
                                                            <td class=" " style="width:24.3%; padding:2px;text-align:right;"><small style="font-size:10px;">@item.SGSTAmount</small></td>
                                                            <td style="width:15%;padding:2px ;text-align:right;"><small style="font-size:10px;">@item.CGST</small></td>
                                                            <td style="width:24.3%;padding:2px; text-align:right;"><small style="font-size:10px;">@item.CGSTAmount</small></td>
                                                            <td style="width:15%; padding:2px;text-align:right;"><small style="font-size:10px;">@item.IGST</small></td>
                                                            <td class=" " style="width:24.3%; padding:2px;text-align:right;"><small style="font-size:10px;">@item.IGSTAmount</small></td>
                                                        </tr>*@
                                                    <tr>

                                                        <th style="data-rel=" width="10%" tooltips" title="Rate(%)"><small style="font-size:8px">@(item.GSTPer/2)</small></th>
                                                        <th width="20%"><small style="font-size: 8px; text-align: right">@(item.GSTAmount/2)</small></th>
                                                        <th style="data-rel=" width="10%" tooltips" title="Rate(%)"><small style="font-size:8px">@(item.GSTPer/2)</small></th>
                                                        <th width="20%"><small style="font-size:8px;text-align:right">@(item.GSTAmount/2)</small></th>
                                                        <th style="data-rel=" width="10%" tooltips" title="Rate(%)"><small style="font-size:8px">@item.GSTPer</small></th>
                                                        <th width="20%"><small style="font-size: 8px; text-align: right">@item.GSTAmount</small></th>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </td>
                                        <td style="text-align: center;"><span class="tax-amount"> @item.TaxableAmount</span></td>
                                        <td style="text-align: center; border-right: none;"><span><span class="prototal"> @item.DP</span></span></td>

                                    </tr>
                                }

                            </tbody>



                        </table>

                    </div>
                </div>
                <br />
                <div class="row-fluid">

                    <div class="span8" style="">
                        <table class="table inventory" style="border-bottom: 1px solid ;" cellspacing="0" cellpadding="0">
                            <thead>
                                <tr>
                                    <td colspan="4">
                                        Amount In Words : @Model.OrderDetails.InWordDP only.
                                    </td>

                                </tr>
                                <tr>
                                    <td colspan="4">
                                        BV In Words : @Model.OrderDetails.InWordBV only.
                                    </td>

                                </tr>
                                <tr style="  background: #f1f1f1;border-right: 1px solid #000;border-bottom: 1px solid #000;">
                                    <th style="text-align:left">
                                        Pay Mode
                                    </th>
                                    <th style="text-align:left">Transactio No</th>
