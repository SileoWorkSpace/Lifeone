@model LifeOne.Models.HomeManagement.HEntity.MultiUserIdList
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style type="text/css">
    .login-inner-form {
        padding: 25px 0 10px 0;
    }

    .login-12 .login-inner-form .details {
        padding: 30px 30px 30px;
    }

    .login-inner-form h3 {
        margin: 0 0 0px;
        font-size: 18px;
        font-weight: 600;
        font-family: 'Open Sans', sans-serif;
        color: #272323;
    }

    .card-body {
        padding: 5px 15px;
    }

    .badgeUid {
        background: #f26122;
        color: #fff;
    }

    .badgebid {
        background: #f6af1b;
        color: #fff;
    }
</style>





<script>
        var status =@ViewBag.Status;
        if (status != null) {
            if (status=="1") {
                setTimeout(() => {
                    location.href = "/Home/Login"
                }, 4000);
            }
        }

</script>


<div class="site-main">
    <div class="ttm-row loginId padding-section">
        <div class="container">
            <div class="">
                @using (Html.BeginForm("Login", "Home", FormMethod.Post))
                {
                    <input type="hidden" value="" name="Login" id="Login" />
                    <input type="hidden" value="@Session["_P"]" name="password" />
                    <input type="radio" checked hidden value="true" name="IsMultipe" />
                    <button type="submit" id="LoginBtn" hidden> login</button>
                }
                <div class="col-md-12">
                    <div class="row login-box-12">
                        <div class="col-lg-12 col-sm-12 col-pad-0 align-self-center">
                            <div class="login-inner-form">
                                <div class="details">
                                    <h3>There is multiple records please choose one of them</h3>
                                    @if (Model != null)
                                    {
                                        int i = 0;
                                        foreach (var v in Model._UserList)
                                        {

                                            if (!v.IsPasswordChange)
                                            {
                                                <a href="/Home/_SetNewPassword?i=@i" id="@v.LoginId">
                                                    <div class="card m-3">
                                                        <div class="card-body">
                                                            <div class="row">
                                                                <div class="col-md-11 text-left">
                                                                    <h3>
                                                                        @v.Name  <br />
                                                                        <em>
                                                                            <small>@v.LoginId</small>

                                                                            @if (!v.IsBlocked)
                                                                            {<small><label class="badge badge-success">Active</label></small>}
                                                                            else
                                                                            {
                                                                                <small><label class="badge badge-danger">InActive</label></small>
                                                                            }

                                                                            @if (!v.IsBusinessId)
                                                                            {<span class="pull-right"><label class="badge badgeUid">UID</label></span>}
                                                                            else if (v.IsBusinessId)
                                                                            { <span class="pull-right"><label class="badge badgebid">BID</label></span>}


                                                                        </em>
                                                                    </h3>
                                                                </div>
                                                                <div class="col-md-1 text-right pt-15">
                                                                    <i class="fa fa-angle-right fa-2x" aria-hidden="true"></i>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </a>
                                            }
                                            else
                                            {
                                                <a href="/Home/_Login?_=@v._LoginId" id="@v.LoginId">
                                                    <div class="card m-3">
                                                        <div class="card-body">
                                                            <div class="row">
                                                                <div class="col-md-11 text-left">
                                                                    <h3>
                                                                        @v.Name  <br />
                                                                        <em>
                                                                            <small>@v.LoginId</small>

                                                                            @if (!v.IsBlocked)
                                                                            {<small><label class="badge badge-success">Active</label></small>}
                                                                            else
                                                                            {
                                                                                <small><label class="badge badge-danger">InActive</label></small>
                                                                            }
                                                                            @if (!v.IsBusinessId)
                                                                            {<span class="pull-right"><label class="badge badgeUid">UID</label></span>}
                                                                            else if (v.IsBusinessId)
                                                                            { <span class="pull-right"><label class="badge badgebid">BID</label></span>}

                                                                        </em>
                                                                    </h3>
                                                                </div>
                                                                <div class="col-md-1 text-right pt-15">
                                                                    <i class="fa fa-angle-right fa-2x" aria-hidden="true"></i>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </a>
                                            }
                                            i = i + 1;
                                        }
                                    }


                                    <p class="mb-2"><button class="ttm-btn ttm-btn-size-md ttm-btn-bgcolor-skincolor" href="javascript:void(0)" onclick="window.history.go(-1); return false;"> Go Back</button></p>

                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>

    <section class="ttm-row history-section bg-img3 ttm-bgcolor-grey ttm-bg ttm-bgimage-yes clearfix">
        <div class="container">
