
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
<title>LifeOne  Culture of "Being Nature"</title>
<!-- favicon icon -->
<link rel="shortcut icon" href="~/landing-page-files/favicon.png" />
<link rel="preconnect" href="https://fonts.gstatic.com">
<link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,400;1,500&display=swap" rel="stylesheet">
<style>
body {
	font-family: 'Roboto', sans-serif;
}
#myVideo {
	position: fixed;
	right: 0;
	bottom: 0;
	min-width: 100%;
	min-height: 100%;
}
.content {
	position: relative;
	color: #f1f1f1;
	height:84vh;
	width: 100%;
}
.video-logo {
	width: 300px;
	height: 300px;
	border-radius: 50%;
	overflow: hidden;
	margin: 0 auto;
	margin-top: 80px;
	background: #fff;
	border: 10px double #e25c14;
	box-sizing: border-box;
}
.video-logo video {
	width: 100%;
	height: 100%;
}
.video-logo-block {
	margin-top: 65px;
}
.culturebeing {
	margin: 50px auto 0;
	width: auto;
	text-align: center;
	position: relative;
}
.culturebeing h2 {
	font-size: 30px;
	font-style: italic;
}
.culturebeing p {
	font-size: 20px;
	padding: 0px 0
}
.culturebeing p img {
	width: 56px;
	margin-top: 30px
}
.culturebeing p span {
	padding: 0 25px;
}

@@media only screen and (max-width: 700px) {
.video-logo {
	margin-top: 60px;
}
.culturebeing {
	margin: 50px auto 0;
}
.culturebeing p img {
	width: 56px;
	margin-top: 10px
}
.culturebeing p {
	font-size:16px; 
}
.culturebeing p span {
	padding: 0 10px;
}
}
</style>
