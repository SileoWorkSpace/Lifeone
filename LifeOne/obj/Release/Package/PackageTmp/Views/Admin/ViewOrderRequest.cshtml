
@{
    ViewBag.Title = "ViewOrderRequest";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div>
    <section class="content-header">
        <div class="col-md-4">
            <h1>F Order Request Approval<small></small> </h1>
            <ol class="breadcrumb" style="clear:both;background:none; padding:0px;">
                <li><a href="#-">Home</a></li>
                <li><a href="#-">Admin</a></li>
                <li class="active">Order Details </li>
            </ol>
        </div>
        <div class="col-md-4">
            <div class="psearch">
                <select class="form-control" style="width:30%; float:left; border-right:0px;border-radius:2px 0 0 2px">
                    <option value="">--Select--</option>
                    <option value="LoginId">Login Id</option>
                    <option value="Mobile">Mobile</option>
                    <option value="Name">Name</option>
                </select>
                <div class="input-group" style="display:block; width:70%; float:left;">

                    <input type="text" id="search" name="search" class="form-control rsearch">
                    <button class="btn btn-success" style="display:block !important;"><i class="fa fa-search"></i></button>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="pbutton">
                <div class="right text-right">
                    <span>
                        <button type="button" class="btn btn-success btn-rounded btngreen"><i class="fa fa-file-excel-o"></i></button>
                    </span>
                </div>
            </div>
        </div>
    </section>
    <section class="content">
        <div id="no-more-tables">
            <form id="form1" method="post">

                <div class="row">
                    <div class="col-xs-12">
                        <div class="box">
                            <div class="box-body">
                                <table id="myTable" class="table small table-hover">
                                    <thead>
                                        <tr>
                                            <th><input type="checkbox" id="chkAll" /></th>
                                            <th> #Sn.</th>
                                            <th>Franchise Name</th>
                                            <th>Total Item</th>
                                            <th>Total Amount</th>
                                            <th>Request Date</th>
                                            <th>Status</th>
                                            <th>Approve Date</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                        </tr>
                                    </tbody>
                                </table>
                                <div>
                                    <button id="btn" ng-click="CreateOrder()" class="btn btn-success pull-right" style="margin:10px">Create Order</button>
                                </div>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>
                    <!-- /.col -->
                </div>
            </form>
        </div>
    </section>
</div>

