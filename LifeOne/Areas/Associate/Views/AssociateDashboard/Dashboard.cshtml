@model LifeOne.Models.AssociateManagement.AssociateEntity.AssociateDashboardWeb
@using LifeOne.Models.Manager
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_AssociateLayout.cshtml";
}
<style>
    .card-header, .card-title {
        margin-top: 0;
        margin-bottom: 0;
    }

    h5.card-title.header-title.border-bottom.p-3.mb-0 {
        margin: 0px !important;
        padding: 5px 0 10px !important;
    }

    .card-body {
        padding: 0px 10px 10px;
        min-height: 304px;
    }

    .min-auto {
        min-height: auto;
    }

    .card hr {
        margin: 0px;
    }

    .p-3 {
        padding: 15px 20px !important;
    }

    .progress {
        background-color: #d6d6d6;
    }

    .pt--0 {
        padding-top: 0px !important;
    }
</style>
<div class="content">
    <div class="content-wrapper-before"></div>
    <div class="container-fluid">

        <div class="row page-title align-items-center">
            <div class="col-sm-12 col-xl-6">
                <h4 class="mb-1 mt-0 text-white">
                    Welcome @SessionManager.Name<br>
                    @*<small>@SessionManager.PerformanceLevel</small>*@
                </h4>
            </div>
        </div>

        <!-- content -->
        <div class="row">
            <div class="col-md-6 col-xl-3">
                <div class="card" style="background: #ff6780">


                    <div class="card-body p-0 min-auto">
                        <div class="media p-3">
                            <div class="media-body">
                                <span class="text-uppercase font-size-12 font-weight-bold text-white">Closing Time Left </span>
                                <a href="#"><h2 class="mb-0 text-white" id="box_header"></h2></a>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-xl-3">
                <div class="card" style="background: #ecac48">


                    <div class="card-body p-0 min-auto">
                        <div class="media p-3">
                            <div class="media-body">
                                <span class="text-uppercase font-size-12 font-weight-bold text-white">Matching </span>
                                <a href="/Payout-Report"><h2 class="mb-0 text-white">@Model.MatchingPoints</h2></a>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-6 col-xl-3">
                <div class="card" style="background:#69abff">

                    <div class="card-body p-0 min-auto">
                        <div class="media p-3">
                            <div class="media-body">
                                <span class="text-uppercase font-size-12 font-weight-bold text-white"> Today's Income</span>
                                <a href="/Payout-Report"><h2 class="mb-0 text-white">@Model.TodaysIncome</h2></a>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-xl-3">
                <div class="card" style="background:#36cd9a">


                    <div class="card-body p-0 min-auto">
                        <div class="media p-3">
                            <div class="media-body">
                                <span class="text-uppercase font-size-12 font-weight-bold text-white">Total Income </span>
                                <a href="/Payout-Report"><h2 class="mb-0 text-white">@Model.TotalIncome</h2></a>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- stats + charts -->
        <div class="row">
            <div class="col-md-3 col-lg-3">
                <div class="card">
                    <div class="card-header">
                        <h5 class="card-title header-title mb-0 text-white">Team Details</h5>
                    </div>
                    <div class="card-body p-0 min-auto">
                        <div class="media p-3" style="padding-top: 0px !important;">
                            <div class="media-body">
                                <h2 class="mb-0"> @Model.TotalActive</h2>
                                <a href="/My-Downline?Status=P">
                                    <span class="text-uppercase font-size-12 font-weight-bold text-success">Active Direct Seller</span>
                                </a>
                            </div>
                        </div>
                    </div>
                    <hr />
                    <div class="card-body p-0 min-auto">
                        <div class="media p-3" style="padding-top:5px !important;">
                            <div class="media-body">
                                <h2 class="mb-0"> @Model.TotalInactive</h2>
                                <a href="/My-Downline?Status=T"><span class="text-uppercase font-size-12 font-weight-bold text-danger">InActive Direct Seller</span></a>
                            </div>
                        </div>
                    </div>
                    <hr />
                    <div class="card-body p-0 min-auto">
                        <div class="media p-3">
                            <div class="media-body">
                                <a href="/My-Downline"><h2 class="mb-0"> @Model.TotalTeam</h2></a>
                                <a href="/My-Downline"><span class="text-uppercase font-size-12 font-weight-bold">Total Direct Seller</span></a>
                            </div>


                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-9">
                <div class="row">
                    <div class="col-md-6">
