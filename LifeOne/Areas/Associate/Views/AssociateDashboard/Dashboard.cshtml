        <div class="row">
            <div class="col-md-3 col-lg-3">
                <div class="card">
                    <div class="card-header">
                        <h5 class="card-title header-title mb-0 text-white">Payout Income</h5>
                    </div>
                    <div class="card-body p-0">
                        @if (Model.payoutSummaries.Count() > 0)
                        {
                            int SNo = 1;
                            foreach (var item in Model.payoutSummaries)
                            {
                                <div class="media p-3" style="padding-top:10px !important;">
                                    <div class="media-body">
                                        <h2 class="mb-0 text-muted"><span class="text-success">@item.Paid</span> / <span class="text-danger">@item.Unpaid</span> </h2>
                                        <span class="text-uppercase font-size-12 font-weight-bold">@item.Income</span>
                                    </div>
                                </div>
                                <hr />
                            }
                        }


                    </div>



                </div>
            </div>
            <div class="col-md-9 col-lg-9">
                <div class="row">
                    <div class="col-md-6">
                        <div class="card">
                            <div class="card-header">
                                <h5 class="card-title header-title mb-0 text-white" style="display:inline;">Direct Team</h5> <a href="/My-Direct" class="pull-right">View All</a>
                            </div>
                            <div class="card-body" style="max-height: 300px; overflow-y: scroll;">
                                @if (Model.lstAssociates.Count() > 0)
                                {
                                    int SNo = 1;
                                    foreach (var item in Model.lstAssociates)
                                    {
                                        <div class="media border-top" style="padding:10px 0 0;">
                                            <img src="@item.profilepic" class="avatar rounded mr-3" alt="shreyu">
                                            <div class="media-body">
                                                <h6 class="mt-1 mb-0 font-size-15">@item.Name <span class="pull-right font-size-13 text-muted">@item.JoiningDate</span></h6>
                                                <h6 class="text-muted font-weight-normal mt-1 mb--0">
                                                    @item.LoginId
                                                    @if (item.ActiveStatus == "Active")
                                                    {

                                                        <span><label class="badge badge-success font-size-10">Active</label></span>
                                                    }
                                                    @if (item.ActiveStatus == "InActive")
                                                    {

                                                        <span><label class="badge badge-danger font-size-10">InActive</label></span>
                                                    }
                                                </h6>
                                            </div>

                                        </div>



                                    }
                                }



                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-6">
                        <div class="card">
                            <div class="card-header">
                                <h5 class="card-title header-title mb-0 text-white">News &amp; Informaton</h5>
                            </div>
                            <div class="card-body">
                                <div class="newslist">
                                    <ul class="slimscroll" style="max-height: 260px;min-height: 260px;">
                                        @if (Model.lstNews.Count > 0)
                                        {
                                            foreach (var item in Model.lstNews)
                                            {
                                                <li class="clearfix">
                                                    <p><strong>@item.NewsHeading</strong></p>
                                                    <p class="description">@item.News</p>
                                                    <p class="font-size-12"><em>Admin</em><small class="font-size-12"> <i class="fa fa-calendar"></i>@item.Date</small></p>
                                                </li>
                                            }
                                        }
                                    </ul>
                                </div>
                            </div>


                        </div>
                    </div>
                </div>
            </div>






        </div>
        <div class="row">

            <div class="col-lg-12 stretch-card">
                <div class="card">
                    <div class="card-body p-0">

                        <div class="table-responsive" id="testTable">
                            <table class="table color-table inverse-table table-striped">
                                <thead>
                                    <tr>
                                        <th>S. No.</th>
                                        <th>Reward Name</th>
                                        <th>Tours</th>
                                        <th>Left Point</th>
                                        <th>Right Point</th>
                                        @*<th>Matching Target Point</th>*@
                                        <th>Status</th>
                                        <th>Acheived On</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model != null)
                                    {
                                        if (Model.Rewardslst != null)
                                        {
                                            int SNO = 0;
                                            foreach (var item in Model.Rewardslst)
                                            {
                                                <tr>
                                                    <td>@(SNO = SNO + 1)</td>
                                                    @if (item.RewardImg == "" || item.RewardImg == "NULL")
                                                    {
                                                        <td></td>
                                                    }
                                                    else
                                                    {
                                                        <td>
                                                            <img src="@item.RewardImg" class="reimg" />
                                                            <br />
                                                            @item.RewardName
                                                        </td>

                                                    }
                                                    @if (item.ToursImage == "" || item.ToursImage == "NULL")
                                                    {
                                                        <td></td>
                                                    }
                                                    else
                                                    {
                                                        <td><img src="@item.ToursImage" class="reimg" /><br />@item.ToursName</td>

                                                    }
                                                    <td>
                                                        @item.LeftBusiness
                                                        @if (!string.IsNullOrEmpty(item.BalanceLeft))
                                                        {
                                                            <br />
                                                            <span>Balance Left: <label class="badge badge-success">@item.BalanceLeft</label></span><br />
                                                        }
                                                    </td>
                                                    <td>
                                                        @item.RightBusiness
                                                        @if (!string.IsNullOrEmpty(item.BalanceRight))
                                                        {
                                                            <br />
                                                            <span>Balance Right: <label class="badge badge-success">@item.BalanceRight</label></span>
                                                        }
                                                    </td>
                                                    @*<td>
                                @item.TargetPoint
                            </td>*@
                                                    @*<td>@item.Rank</td>
                            <td>@item.Designation</td>*@

                                                    <td>
                                                        @if (item.Status == "Achieved")
                                                        {
                                                            <span class="badge badge-success">@item.Status</span>
                                                        }
                                                        else if (item.Status == "Pending")
                                                        {
                                                            <span class="badge badge-warning">@item.Status</span>
                                                        }
                                                        else if (item.Status == "Skipped")
                                                        {
                                                            <span class="badge badge-danger">@item.Status</span>
                                                        }
                                                        else if (item.Status == "Claimed")
                                                        {
                                                            <span class="badge badge-info">@item.Status</span>
                                                        }
                                                        else if (item.Status == "Claim Denied")
                                                        {
                                                            <span class="badge badge-red">@item.Status</span>
                                                        }

                                                        else if (item.Status == "Approved By Admin")
                                                        {
                                                            <a class="text-primary" style="cursor:pointer;text-decoration:underline" onclick="ShowBankDetails(@item.PK_RId)"><span class="badge badge-primary">@item.Status</span></a>
                                                        }
                                                    </td>
                                                    <td>@item.AchievedOn</td>

                                                </tr>
                                            }
                                        }
                                    }

                                </tbody>

                            </table>
                            @*<div>
                                    <h4>1 Point = Rs. 50000 business.</h4>
                                </div>*@
                        </div>
                    </div>
                </div>
            </div>
        </div>
