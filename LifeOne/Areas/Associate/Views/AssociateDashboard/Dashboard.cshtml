                            <div class="card-header" style="background: #fff; border-radius: 0;">
                                <h5 class="card-title header-title mb-0 text-center">All Business</h5>
                            </div>
                            <div class="card-body p-0 min-auto" style=" background: #fff;">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="text-center">
                                            <h3 class="text-muted text-uppercase font-size-14 font-weight-bold">Left</h3>
                                            <h2>@Model.LeftBusiness</h2>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="text-center">
                                            <h3 class="text-muted text-uppercase font-size-14 font-weight-bold">Right</h3>
                                            <h2>@Model.RightBusiness</h2>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <hr />
                            <div class="card-header">
                                <h5 class="card-title header-title mb-0 text-center text-white">Current Closing Business</h5>
                            </div>
                            <div class="card-body p-0 min-auto">
                                <div class="row">
                                    @{
                                        decimal Progbar = 0;


                                    }




                                    <div class="col-md-6">
