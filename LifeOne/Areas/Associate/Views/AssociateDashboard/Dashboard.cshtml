                                            <h3 class="text-uppercase font-size-12 font-weight-bold">Left</h3>
                                            <h2>@Model.CurrentBusinessLeft</h2>
                                            <div class="progress my-2" style="height: 10px;">
                                                <div class="progress-bar bg-danger" role="progressbar" style="width:@Progbar%"
                                                     aria-valuenow="60" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div>
                                            @*<h3 class="text-uppercase font-size-14 font-weight-bold">@Model.PaidLeg1</h3>*@

                                            <h3 class="text-uppercase font-size-14 font-weight-bold">@Model.PreLeftBV</h3>
                                        </div>
                                    </div>

                                    @{
                                        decimal Progbar1 = 0;


