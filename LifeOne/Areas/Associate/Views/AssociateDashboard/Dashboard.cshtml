                                            <h3 class="text-uppercase font-size-14 font-weight-bold">@Model.PreRightBV</h3>
                                        </div>
                                    </div>
                                </div>

                            </div>

                        </div>

                    </div>
                    <div class="col-md-6">
                        <div class="card">
                            <div class="card-header" style="background: #fff; border-radius: 0;">
                                <h5 class="card-title header-title mb-0 text-center">Current Business Repurchase</h5>
                            </div>
                            <div class="card-body p-0 min-auto" style="background: #fff;">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="text-center">
                                            <h3 class="text-muted text-uppercase font-size-14 font-weight-bold">Left</h3>
                                            <h2>@Model.RepurchaseCurrentBusinessLeft</h2>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="text-center">
                                            <h3 class="text-muted text-uppercase font-size-14 font-weight-bold">Right</h3>
                                            <h2>@Model.RepurchaseCurrentBusinessRight</h2>


                                        </div>
                                    </div>

                                </div>
                            </div>
                            <hr />
                            <div class="card-header">
                                <h5 class="card-title header-title mb-0 text-center text-white">Team Business Repurchase</h5>
                            </div>
                            <div class="card-body p-0 min-auto">
                                <div class="row">

                                    @{

                                        decimal RLProgressbar = 0;
                                        decimal RRProgressbar = 0;

                                    }
                                    @if (Convert.ToDecimal(@Model.RepurchaseLeftBusiness) > Convert.ToDecimal(@Model.RepurchaseRightBusiness))
                                    {
                                        RLProgressbar = ((Convert.ToDecimal(@Model.RepurchaseRightBusiness) * 100) / Convert.ToDecimal(@Model.RepurchaseLeftBusiness));

                                    }
                                    else
                                    {
                                        RLProgressbar = (Convert.ToDecimal(@Model.RepurchaseLeftBusiness) * 100);

                                    }
                                    @if (Convert.ToDecimal(@Model.RepurchaseRightBusiness) > Convert.ToDecimal(@Model.RepurchaseLeftBusiness))
                                    {
                                        RRProgressbar = ((Convert.ToDecimal(@Model.RepurchaseLeftBusiness) * 100) / Convert.ToDecimal(@Model.RepurchaseRightBusiness));

                                    }
                                    else
                                    {
                                        RRProgressbar = (Convert.ToDecimal(@Model.RepurchaseRightBusiness) * 100);

                                    }
                                    <div class="col-md-6">
                                        <div class="p-3 text-center pt--0">
                                            <h3 class="text-uppercase font-size-12 font-weight-bold">Left</h3>
                                            <h2>@Model.RepurchaseLeftBusiness</h2>
                                            <div class="progress my-2" style="height: 10px;">
                                                <div class="progress-bar bg-danger" role="progressbar" style="width: @RLProgressbar%"
                                                     aria-valuenow="60" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div>
                                            @*<h3 class="text-uppercase font-size-14 font-weight-bold">@Model.RepurchasePaidLeg1</h3>*@

                                            <h3 class="text-uppercase font-size-14 font-weight-bold">@Model.RepurchaseCarryforwordLeft</h3>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="p-3 text-center pt--0">
                                            <h3 class="text-uppercase font-size-12 font-weight-bold">Right</h3>
                                            <h2>@Model.RepurchaseRightBusiness</h2>
                                            <div class="progress my-2" style="height: 10px;">
                                                <div class="progress-bar bg-danger" role="progressbar" style="width:@RRProgressbar%"
                                                     aria-valuenow="60" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div>
                                            @*<h3 class="text-uppercase font-size-14 font-weight-bold">@Model.RepurchasePaidLeg2</h3>*@
                                            <h3 class="text-uppercase font-size-14 font-weight-bold">@Model.RepurchaseCarryforwordRight</h3>
                                        </div>
                                    </div>
                                </div>

                            </div>

                        </div>

                    </div>

                </div>
            </div>


