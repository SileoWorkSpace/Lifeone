<div class="fixedEmail" id="sendEmail">
    <div class="modal-dialog">
        <div class="modal-content box box-primary box-solid fixedEmail">
            <div class="card-header with-border bg-info p-1 d-flex align-items-center justify-content-between">
                <h5 class="card-title mb-0 text-white">Send Email</h5>
                <div class="card-tools">
                    <button class="btn btn-box-tool minimize-modal btn-xs">
                        <i class="fa fa-minus"></i>
                    </button>
                    <button class="btn btn-box-tool close-modal btn-xs">
                        <i class="fa fa-remove"></i>
                    </button>
                </div>
            </div>
            <div class="card-body p-1">
                <div class="form-group position-relative">
                    <div class="input-group input-group-sm">
                        <div class="input-group-prepend">
                            <span class="input-group-text">To:</span>
                        </div>
                        <input id="txtEmail" name="txtEmail" class="form-control" disabled="" />
                        <div class="input-group-append">
                            <span class="input-group-text openCCBox">CC</span>
                            <span class="input-group-text openBCCBox">BCC</span>
                        </div>
                    </div>
                </div>
                <div class="form-group CCBox">
                    <div class="input-group input-group-sm flex-nowrap">
                        <div class="input-group-prepend">
                            <span class="input-group-text">CC:</span>
                        </div>
                        @*<select id="txtMailCC"  class="js-example-basic-multiple form-control" name="states[]" multiple="multiple">
                            </select>*@
                        @Html.DropDownList("txtMailCC", (IEnumerable<SelectListItem>)ViewBag.Email, new { @class = "js-example-basic-multiple form-control", @multiple = "multiple" })

                    </div>
                </div>
                <div class="form-group BCCBox">
                    <div class="input-group input-group-sm flex-nowrap">
                        <div class="input-group-prepend">
                            <span class="input-group-text">BCC:</span>
                        </div>

                        @*<select id="txtMailBCC" class="js-example-basic-multiple form-control" name="states[]" multiple="multiple">
                            </select>*@
                        @Html.DropDownList("txtMailBCC", (IEnumerable<SelectListItem>)ViewBag.Email, new { @class = "js-example-basic-multiple form-control", @multiple = "multiple" })
                        @*@Html.DropDownList("txtMailBCC", new SelectList(ViewBag.Email), "Please select", new { @id = "txtMailBCC", @class = "form-control" })*@
                    </div>
                </div>
                <div class="form-group position-relative">
                    <div class="input-group input-group-sm">
                        <div class="input-group-prepend">
                            <span class="input-group-text">Subject:</span>
                        </div>
                        <input id="txtSubject" name="txtSubject" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <textarea id="compose-textarea" name="compose-textarea" class="form-control" placeholder="Type message here..."></textarea>
                </div>
            </div>
            <div class="card-footer bg-light-blue p-2">
                <button class="btn btn-default btn-xs text-dark close-modal">
                    <i class="fa fa-times"></i> Discard
                </button>
                <button id="SendEmail" class="btn btn-default btn-xs  text-dark">
                    <i class="fa fa-envelope-o"></i> Send
                </button>
                <div class="btn btn-default btn-xs btn-file attachment-btn text-dark" id="dvAttachment">
                    <i class="fa fa-paperclip"></i> Attachment (Max. 20MB)
                    <input type="file" id="attachment" name="attachment" onchange="FileUpload('Slip',this)" />
                    <input type="hidden" id="hdnfilepath" name="hdnfilepath" />
                </div>
            </div>
        </div>
    </div>
</div>

<script>
    $('.selectdate').datepicker({
        autohide: true,
        format: 'dd/mm/yyyy',
    }).on('changeDate', function (e) {
        $(this).datepicker('hide');
    });

    var Fk_MemId = [];
    var Email = [];
    var NameNotification = [];
    $('.js-example-basic-multiple').select2();
    $(".openCCBox").click(function () {
        $('.CCBox').toggle();
    });
    $(".openBCCBox").click(function () {
        $('.BCCBox').toggle();
    });
    //Email
    $(".minimize-modal").click(function () {
        $('.fixedEmail .card-body').toggle();
        $('.fixedEmail .card-footer').toggle();
    });
    $(".close-modal").click(function () {
        $('.fixedEmail').hide();
    });
    $('#chkParent').on('click', function () {
        if (this.checked) {
            $('.chkchild').each(function () {
                this.checked = true;
                var name = [];
                $.each($("input[name='chk']:checked"), function () {
                    var data = $(this).parents('tr:eq(0)');
                    name.push($(data).find('td:eq(3)').text());
                });
                $("#txtname").val(name);

            });
        } else {
            $('.chkchild').each(function () {
                this.checked = false;
            });
        }
    });
    $(document).on("click", ".chkchild", function () {
        if ($('.chkchild:checked').length == $('.chkchild').length) {
            var name = [];
            $.each($("input[name='chk']:checked"), function () {
                var data = $(this).parents('tr:eq(0)');
                name.push($(data).find('td:eq(3)').text());
            });
            $("#txtname").val(name);
            $("#MemberId").val(MemberId);
            $('#chkParent').prop('checked', true);
        } else {
            var name = [];
            $.each($("input[name='chk']:checked"), function () {
                var data = $(this).parents('tr:eq(0)');
                name.push($(data).find('td:eq(3)').text());
            });
            $("#txtname").val(name);
            $('#chkParent').prop('checked', false);
        }
    });
    function Notification() {
        NameNotification = [];
        if ($("input[name='chk']:checked").val() != undefined) {
            $.each($("input[name='chk']:checked"), function () {
                var data = $(this).parents('tr:eq(0)');
                NameNotification.push($(data).find('td:eq(3)').text());
            });
            $("#txtnameNotification").val(NameNotification);
            $('#sendNotification').modal('show');
        } else {
            swal("", "No record selected !!!.", "warning")
            return false;
        }
    }
    function SMS() {
        if ($("input[name='chk']:checked").val() != undefined) {
            $('#sendSMS').modal('show');
        } else {
            swal("", "No record selected !!!.", "warning")
            return false;
        }
    }
    function sendEmail() {
        Email = [];
        Fk_MemId = [];
        if ($("input[name='chk']:checked").val() != undefined) {
            $.each($("input[name='chk']:checked"), function () {
                var data = $(this).parents('tr:eq(0)');
                Email.push($(data).find('td:eq(10)').text());
                Fk_MemId.push($(data).find('td:eq(9)').text());
            });
            $("#txtEmail").val(Email);
            $("#txtMailCC").val(Email);
            $("#txtMailBCC").val(Email);
            $('.fixedEmail').show();
        }
        else {
            swal("", "No record selected !!!.", "warning");
            $('.fixedEmail').hide();
            return false;
        }
    }
    $("#SendSMS").click(function () {
        var MemberId = [];
        var message = $("#txtsmsnotification").val();
        $.each($("input[name='chk']:checked"), function () {
            var data = $(this).parents('tr:eq(0)');
            MemberId.push($(data).find('td:eq(9)').text());
        });
        var model = {
            SMSContent: message,
            LstMemberId: MemberId
        };
        if (message != "") {
            $.ajax({
                type: "POST",
                url: "/Associate/AssociateTeam/SaveSMS",
                data: JSON.stringify(model),
                contentType: 'application/json; charset=utf-8',
                dataType: 'json',
                success: function (data) {
                    if (data != null && data != "") {
                        if (data.Flag == '1') {
                            swal("", data.Msg, "success");
                            window.setTimeout(function () {
                                window.location.href = "/Associate/AssociateTeam/MyDownline";
                            }, 3000);
                        }
                        else {
                            swal("", data.Msg, "warning");
                            window.setTimeout(function () {
                                window.location.href = "/Associate/AssociateTeam/MyDownline";
                            }, 3000);
                        }
                    }
                },
                failure: function (data) {
                    swal("", "Something went wrong!!!", "error");
                    window.setTimeout(function () {
                        window.location.href = "/Associate/AssociateTeam/MyDownline";
                    }, 3000);
                },
                error: function (data) {
                    swal("", "Something went wrong!!!", "error");
                    window.setTimeout(function () {
                        window.location.href = "/Associate/AssociateTeam/MyDownline";
                    }, 3000);
                }
            });
        }
        else {
            swal("", "Please type some content", "warning");
            $("#txtsmsnotification").val('');

        }
    });
    $("#SendNotification").click(function () {
        var MemberId = [];
        var Notificationmessage = $("#txtnotification").val();
        var Title = $("#txtTitle").val();
        if ($("#txtTitle").val() == "") {
            swal("", "Please enter title", "warning");
            return false;
        }
        $.each($("input[name='chk']:checked"), function () {
            var data = $(this).parents('tr:eq(0)');
            MemberId.push($(data).find('td:eq(9)').text());
        });
        var model = {
            Notificationmessage: Notificationmessage,
            Title: Title,
            LstMemberId: MemberId
        };
        if (Notificationmessage != "") {
            $.ajax({
                type: "POST",
                url: "/Associate/AssociateTeam/SaveNotification",
                data: JSON.stringify(model),
                contentType: 'application/json; charset=utf-8',
                dataType: 'json',
                success: function (data) {
                    if (data != null && data != "") {
                        if (data.Flag == '1') {
                            swal("", data.Msg, "success");
                            location.reload();
                        }
                        else {
                            swal("", data.Msg, "warning");
                            window.setTimeout(function () {
                                window.location.href = "/Associate/AssociateTeam/MyDownline";
                            }, 3000);
                        }
                    }
                },
                failure: function (data) {
                    swal("", "Something went wrong!!!", "error");
                    window.setTimeout(function () {
                        window.location.href = "/Associate/AssociateTeam/MyDownline";
                    }, 3000);
                },
                error: function (data) {
                    swal("", "Something went wrong!!!", "error");
                    window.setTimeout(function () {
                        window.location.href = "/Associate/AssociateTeam/MyDownline";
                    }, 3000);
                }
            });

        }
        swal("", "Please enter Message", "warning");
    });
    $("#SendEmail").click(function () {
        var EmailMessage = $("#compose-textarea").val();
        var Subject = $("#txtSubject").val();
        var filepath = $("#hdnfilepath").val();
        var Emailccc = [];
        $('#txtMailCC :selected').each(function (i, sel) {
            Emailccc.push($(sel).text());
        });
        //var Emailccc = $("#txtMailCC option:selected").text();

        alert(Emailccc);
        var EmailBcc = $("#txtMailBCC option:selected").text();
        if (EmailMessage != "") {
            if (Email.length > 0) {
                $.ajax({
                    type: "POST",
                    url: "/Associate/AssociateTeam/SaveEMail",
                    data: { Email: Email, EmailContect: EmailMessage, Title: Subject, FilePath: filepath, EmailCC: Emailccc, EmailBCC: EmailBcc, LstMemberId: Fk_MemId },
                    success: function (data) {
                        if (data != null) {
                            if (data.Flag == '1') {
                                swal("", "Email sent successFully!!!", "success");
                                window.setTimeout(function () {
                                    window.location.href = "/Associate/AssociateTeam/MyDownline";
                                }, 3000);
                            }
                        }
                    },
                    failure: function (data) {
                        swal("", "Some Error!!!", "error");
                        window.setTimeout(function () {
                            window.location.href = "/Associate/AssociateTeam/MyDownline";
                        }, 3000);
                    },
