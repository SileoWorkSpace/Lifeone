                                        int SNo = 0;
                                        foreach (var item in Model.Downlines)
                                        {
                                            <tr>
                                                <td>@(SNo = SNo + 1)</td>
                                                <td>
                                                    <span>@item.MemberLoginId</span>
                                                </td>
                                                <td>
