                if (item.District == district) {
                    $("#StateName").val(item.State);
                    $("#Tehsil").val(item.DistrictDetail[0].Taluka);
                    $("#Zone").val(item.DistrictDetail[0].Zone);
                    $('#CountryId').val(1);
                }

            });

        }

