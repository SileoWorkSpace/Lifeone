<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.7.1/css/bootstrap-datepicker.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.7.1/js/bootstrap-datepicker.js"></script>
<script>
    $(document).ready(function () {
        debugger;
        var load = $('#MaritalStatusID').val();
        if (load == "Married") {
            $('#Married').prop("checked", true);
            $('#Div1').show();
        }
        else if (load == "Single") {
            $('#Single').prop("checked", true);
            $('#Div1').hide();
            $('#MarriageAnniversaryID').val('');

        }


    });

        $('.selectdate').datepicker({});
     var ArrFamilyDetails = [];
    $(document).ready(function () {
        //hide Marrital
        debugger;
        $("input[value='@Model.AccountPurpose']").attr('checked', true);
        $("input[value='@Model.RegType']").attr('checked', true);
        $("input[value='@Model.Gender']").attr('checked', true);
        $("input[value='@Model.OrgType']").attr('checked', true);
        $("input[value='@Model.MaritalStatus']").attr('checked', true);
        $('#sponsorName').text('@Model.SponserName');
        if (@Model.Fk_MemId!= null) {
            $("#spanCustAadharImage").css("display", "block");
            $("#spanCustAadharImage").css("color", "green");
            $("#CustAadharImagestatusval").text('@Model.CustAadharImageStatus');
            $("#spanSpouseAadharImages").css("display", "block");
            $("#spanSpouseAadharImages").css("color", "green");
            $("#SpouseAadharImagesstatusval").text('@Model.SpouseAadharImagesStatus');
            $("#CountryId option:selected").text('@Model.CountryId');
            $("#CityID option:selected").text('@Model.DistrictName');
            $("#Zone").text('@Model.Zone');
            $("#spanprofilepic").css("display", "block");
            $("#spanprofilepic").css("color", "green");            
            $("#ProfileImagesstatusval").text('Upload');                                                                          
            if (@Model.lstChild.Count> 0) {

                //table start
                var table = '<table class="table table-bordered success-bordered-table" id="tbodyData">';
                //preparing html table row and data here
                  table += '<thead>' +
                '<tr>' +
                '</tr>' +
                '<tr class="bg-inverse">' +
