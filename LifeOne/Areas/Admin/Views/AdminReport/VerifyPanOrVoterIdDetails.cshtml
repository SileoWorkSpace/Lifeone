@model  LifeOne.Models.AdminManagement.AEntity.VerifyPanOrVoterid
@using LifeOne.Models.Manager;

@{
    ViewBag.Title = "VerifyPanOrVoterIdDetails";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";

}


<style>
    .hide {
        display: none;
    }
</style>


<div class="content">
    <!-- Start Content-->
    <div class="container-fluid">
        <div class="row page-titles">
            <div class="ptitle">
                <h4 class="mb-1 mt-0">Verify KYC</h4>
                @*<nav aria-label="breadcrumb" class="float-left mt-1">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="#">Dashboard</a></li>
                        <li class="breadcrumb-item"><a href="#">KYC</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Verify Pan Or VoterId List</li>
                    </ol>
                </nav>*@
            </div>
            <div class="pbutton">
                <div class="right text-right">
                    <span>
                        @*<a class="advance-search" title="Click here for search" data-toggle="tooltip" data-placement="bottom" href="#-">Search</a>*@
                    </span>


                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <ul class="nav nav-tabs profile-tab" role="tablist">
                            <li class="nav-item"> <a class="nav-link active" data-toggle="tab" href="#home" role="tab">Verify KYC</a> </li>
                            <li class="nav-item"> <a class="nav-link" href="/Admin-ApproveKYC" role="tab">Approved KYC</a> </li>
                            <li class="nav-item"> <a class="nav-link" href="/Admin-DeclineKYC" role="tab">Declined KYC</a> </li>
                        </ul>





                        <div class="row" id="advance-search" style="display:none;">

                            <div class="col-md-12">
                                <div class="alert alert-warning" style="color:#727272 !important">

                                    <form action="" method="post">
                                        <div class="row">
                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label>Login Id</label>
                                                    <input type="text" name="LoginId" id="LoginID" placeholder="Enter Login Id" class="form-control" value="@ViewBag.loginId">

                                                </div>
                                            </div>
                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label>Mobile</label>
                                                    <input type="text" name="Mobile" id="Mobile" placeholder="Enter Mobile No" class="form-control" maxlength="10" onkeypress="return isNumberKey(event)" value="@ViewBag.Mobile">

                                                </div>
                                            </div>


                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label>PAN No</label>
                                                    <input type="text" name="PanCard" id="PanCard" placeholder="Enter PAN No" class="form-control" value="@ViewBag.Pan">

                                                </div>
                                            </div>




                                            <div class="col-12 col-md-2">
                                                <div class="form-group">
                                                    <label style="display:block;"></label>
                                                    <input type="submit" class="btn btn-success " style="margin-top:1.5em;" value="Search" id="BtnSearch">
                                                    <a href="/VerifyKyc" class="btn btn-secondary" style="margin-top:1.5em;">Reset</a>

                                                </div>
                                            </div>

                                        </div>
                                    </form>
                                </div>
                            </div>


                        </div>


                        <div class="tab-content">

                            <div class="tab-pane active" role="tabpanel">
                                <div class="table-responsive" >
                                    <table class="table table-striped table-bordered" style="width:100%" id="example">
                                        <thead>
                                            <tr>
                                                <th>#Sn.</th>
                                                <th>Name</th>
                                                <th>Login Id</th>
                                                <th>Mobile</th>
                                                <th>PAN Card</th>

                                                <th>Address Proof Front</th>
                                                <th>Address Proof Back</th>
                                                <th>Bank</th>
                                                <th>Action</th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (Model != null)
                                            {
                                                if (Model.List != null && Model.List.Count > 0)
                                                {
                                                    int i = 0;
                                                    int page = Model.Pager.CurrentPage;
                                                    page = (page - 1);
                                                    i = ((page) * (Model.Pager.PageSize));


                                                    foreach (var item in Model.List)
                                                    {

                                                        <tr>
                                                            <td align="left">@(i = i + 1)</td>
                                                            <td align="left"><span>@item.Name</span></td>
                                                            <td align="left"><span>@item.LoginId</span></td>
                                                            <td align="left"><span>@item.Mobile</span></td>

                                                            <td align="center">

                                                                @if (item.PanCardStatus == "Pending")
                                                                {


                                                                    @*<a onclick="OpenPanModal('@item.PanCard','@item.PanCardURL')"><i class="fa fa-eye" aria-hidden="true"></i></a>*@
                                                                    <img src="@item.PanImage" onclick="OpenPanModal('@item.PanCard','@item.PanImage')" style="height:50px;width:50px;" />
                                                                    <br />

                                                                    <span class="badge badge-warning">@item.PanCardStatus</span>
                                                                }

                                                                else
                                                                {
                                                                    <img src="@item.PanImage" onclick="OpenPanModal('@item.PanCard','@item.PanImage')" style="height:50px;width:50px;" />

                                                                    <span class="badge badge-success">@item.PanCardStatus</span>
                                                                }

                                                            </td>

                                                            <td align="center">

                                                                @if (item.AddressProofStatus == "Pending")
                                                                {


                                                                    <img src="@item.AadharImage" onclick="OpenVoterModal('@item.AadharImage','@item.AadharNumber')" style="height:50px;width:50px;" />
                                                                    <br />

                                                                    <span class="badge badge-warning">@item.AddressProofStatus</span>
                                                                }

                                                                else
                                                                {
                                                                    <img src="@item.AadharImage" onclick="OpenVoterModal('@item.AadharImage','@item.AadharNumber')" style="height:50px;width:50px;" />
                                                                    <span class="badge badge-success">@item.AddressProofStatus</span>
                                                                }

                                                            </td>
                                                            <td align="center">

                                                                @if (item.AddressProofBackStatus == "Pending")
                                                                {

                                                                    <img src="@item.AadharBackImage" onclick="OpenAadharModal('@item.AadharNumber','@item.AadharBackImage')" style="height:50px;width:50px;" />
                                                                    <br /> <span class="badge badge-warning">@item.AddressProofBackStatus</span>
                                                                }
                                                                else
                                                                {
                                                                    <img src="@item.AadharBackImage" onclick="OpenAadharModal('@item.AadharNumber','@item.AadharBackImage')" style="height:50px;width:50px;" />
                                                                    <br /> <span class="badge badge-success">@item.AddressProofBackStatus</span>
                                                                }

                                                            </td>

                                                            <td align="center">

                                                                @if (item.BankStatus == "Pending")
                                                                {

                                                                    <img src="@item.BankImage" onclick="OpenBankModal('@item.BankImage', '@item.AccountNumber', '@item.IfscCode')" style="height:50px;width:50px;" />

                                                                    <br />

                                                                    <span class="badge badge-warning">@item.BankStatus</span>
                                                                }

                                                                else
                                                                {
                                                                    <img src="@item.BankImage" onclick="OpenBankModal('@item.BankImage','@item.AccountNumber','@item.IfscCode')" style="height:50px;width:50px;" />

                                                                    <span class="badge badge-success">@item.BankStatus</span>
                                                                }

                                                            </td>
                                                            <td align="left">
                                                                @if (PermissionManager.IsActionPermit("Verify KYC", ViewOptions.FormUpdate))
                                                                {
                                                                    <a href="#" onclick="Approve(@item.Fk_MemId,'@item.Type','Approve')" class="btn btn-success btn-sm">Approve</a>
                                                                    <a href="#" onclick="Decline(@item.Fk_MemId,'@item.Type','Decline')" class="btn btn-danger btn-sm">Decline</a>
                                                                }
                                                                @if (item.AddressProofBackStatus == "Pending" || item.AddressProofBackStatus == "Decline")
                                                                {
                                                                    <a href="javascript:void(0);" class="btn btn-warning btn-sm" onclick="UploadKyc(@item.Fk_MemId);">Upload</a>
                                                                }
                                                            </td>



                                                        </tr>

                                                    }
                                                }
                                            }

                                        </tbody>
                                    </table>
                                </div>
                            </div>

                        </div>
                    </div>
                    <footer class="card-footer bg-white p-2">
                        <div class="d-md-flex align-items-center justify-content-between">

                            @if (Model != null)
                            {
                                if (Model.Pager != null)
                                {
                                    if (Model.Pager.EndPage > 1)
                                    {
                                        <div> <small class="text-muted inline">showing @Model.List.Count  of @Model.List[0].TotalRecords items</small> </div>
                                        <div>
                                            <ul class="pagination pagination-sm m-t-none m-b-none">
                                                @if (Model.Pager.CurrentPage > 1)
                                                {
                                                    <li class="page-item">
                                                        <a class="page-link" href="/VerifyKyc?Name=@Model.Name&LoginId=@Model.LoginId&Mobile=@Model.Mobile&PanCard=@Model.PanCard">First</a>
                                                    </li>
                                                    <li class="page-item">
                                                        <a class="page-link" href="/VerifyKyc?page=@(Model.Pager.CurrentPage - 1)&Name=@Model.Name&LoginId=@Model.LoginId&Mobile=@Model.Mobile&PanCard=@Model.PanCard">Previous</a>
                                                    </li>
                                                }
                                                @for (var page = Model.Pager.StartPage; page <= Model.Pager.EndPage; page++)
                                                {
                                                    <li class="page-item @(page == Model.Pager.CurrentPage ? "active" : "")">
                                                        <a class="page-link" href="/VerifyKyc?page=@page&Name=@Model.Name&LoginId=@Model.LoginId&Mobile=@Model.Mobile&PanCard=@Model.PanCard">@page</a>
                                                    </li>
                                                }
                                                @if (Model.Pager.CurrentPage < Model.Pager.TotalPages)
                                                {
                                                    <li class="page-item">
                                                        <a class="page-link" href="/VerifyKyc?page=@(Model.Pager.CurrentPage + 1)&Name=@Model.Name&LoginId=@Model.LoginId&Mobile=@Model.Mobile&PanCard=@Model.PanCard">Next</a>
                                                    </li>
                                                    <li class="page-item">
                                                        <a class="page-link" href="/VerifyKyc?page=@(Model.Pager.TotalPages)&Name=@Model.Name&LoginId=@Model.LoginId&Mobile=@Model.Mobile&PanCard=@Model.PanCard">Last</a>
                                                    </li>
                                                }
                                            </ul>
                                        </div>
                                    }
                                }

                            }

                        </div>
                    </footer>

                </div>
            </div>
        </div>

    </div>
    <!-- container-fluid -->

</div>

