@model FamilyN.Models.AssociateManagement.AssociateEntity.MAssociateAdminInvoice

@{
    Layout = null;
    ViewBag.Title = "MemberTaxInvoice";
}
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width">
    <title>Print Invoice </title>
    <link href="~/Print Invoice_files/responsive.css" rel="stylesheet">
    <link href="~/Print Invoice_files/newInvoiceCss.css" rel="stylesheet">
    <link href="~/Print Invoice_files/font-awesome.css" rel="stylesheet">


    <style>

        .content {
            height: 10in;
        }
    </style>
</head>
<body>


    <div>
        <div class="content">

            <span id="lblBilledBy" style="float: right; display: none;">Invoiced By: FamilyN</span>

            <div id="divcontent">
                <div class="row-fluid">

                    <div class="span12">
                        <table class="table" style="border-top: 0px;" cellspacing="0" cellpadding="0">
                            <tbody>
                                <tr>
                                    <td colspan="2" style="text-align:center;">
                                        <strong><strong style="font-size: small;margin-left:102px;">Tax Invoice</strong></strong>
                                    </td>
                                </tr>
                                <tr style="border-top: 0px;">
                                    <td style="padding: 10px 0px 10px 10px; width: 25% !important; border-top: 0px;">
                                        @*<img src="./Print Invoice_files/logo.png*@
                                        <img src="~/Content/admin-assets/images/logo.png" alt="Saler Logo" style="width: 100%;">
                                    </td>
                                    <td style="text-align: center; padding-right: 10px; border-top: 0px;">

                                        <strong>
                                            Office no. 307, Legacy building, Lunkad Sky Station, Near HDFC Bank Viman Nagar, Pune Maharashtra-411014
                                        </strong><br>
                                                 <strong>Contact: +91 1234567890</strong>&nbsp;|&nbsp;<strong>E-mail: info@familyn.co.in</strong>&nbsp;|&nbsp;
                                        <strong>Website: www.familynbusiness.com</strong><br>

                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="row-fluid">
                    <div class="span12">
                        <table class="table" style="width: 50% !important; float: left; border-right: 1px solid;" cellspacing="0" cellpadding="0">
                            <tbody>
                                <tr>
                                    <th style="border-top: none;">GST Number</th>
                                    <td style="border-top: none; font-weight: 800;">:&nbsp;<label id="lblGSTCopy"></label>09AADCL5023EIZT</td>
                                </tr>
                                <tr>
                                    <th>Invoice Serial Number</th>
                                    <td>:&nbsp;<span id="lblInvoiceNo">@Model.OrderNo</span></td>

                                </tr>
                                <tr>
                                    <th>Invoice Date</th>
                                    <td>
                                        :&nbsp;<span id="lblInvDate">@Model.OrderDate</span>
                                    </td>
                                </tr>


                            </tbody>
                        </table>
                        <table class="table" style="width: 50%;" cellspacing="0" cellpadding="0">
                            <tbody>
                                <tr>
                                    <th style="border-top: none;">Copy</th>
                                    <td style="border-top: none;">:&nbsp;<span id="lblDocNo" style="display: none;">NA</span><span contenteditable="">Original</span></td>
                                </tr>
                                <tr>
                                    <th>Transport Mode</th>
                                    <td>:&nbsp;<span id="lblTransportMode">NA</span></td>
                                </tr>
                                <tr>
                                    <th>Vehicle Number</th>
                                    <td>:&nbsp;<span id="lblVehicalNumber">NA</span></td>
                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="row-fluid">
                    <div class="span12">
                        <table class="table" style="width: 50% !important; float: left; border-right: 1px solid;" cellspacing="0" cellpadding="0">

                            <tbody>
                                <tr>
                                    <th colspan="2" style="border-top: none;  background: #f1f1f1;border-right: 1px solid #000;border-bottom: 1px solid #000;text-align: left;">
                                        Billing Address
                                        <span class="pull-right">OrderNo: <span style="font-size: 10px;">@Model.OrderNo</span></span>
                                    </th>
                                </tr>
                                <tr></tr>
                                <tr>
                                    <th style="border-top: 1px solid;">Name</th>
                                    <td style="border-top: 1px solid;">:&nbsp;<span id="rptBillTo_lblCustomerName_0">@Model.MemberName </span></td>
                                </tr>
                                <tr>
                                    <th>ID</th>
                                    <td>:&nbsp;<span id="rptBillTo_Label1_0">@Model.OrderNo</span></td>
                                </tr>
                                <tr>
                                    <th>Address</th>
                                    <td>:&nbsp;<span id="rptBillTo_lblAddress_0">@Model.Address</span></td>

                                </tr>
                                <tr></tr>



                            </tbody>
                        </table>
                        <table class="table wt-mn" style="width: 50% !important;" cellspacing="0" cellpadding="0">
                            <tbody>
                                <tr>
                                    <th style="border-top: none;  background: #f1f1f1;border-right: 1px solid #000;border-bottom: 1px solid #000;text-align: left;" colspan="2">
                                        Shipping Address

                                    </th>
                                </tr>
                                <tr></tr>
                                <tr>
                                    <th style="border-top: 1px solid;">Payment Mode</th>
                                    <td style="border-top: 1px solid;">:&nbsp;<span id="rptShipTo_lblCustomerName2_0"> @Model.PayMode</span></td>

                                </tr>
                                <tr>
                                    <th>Bank Name</th>
                                    <td>:&nbsp;<label id="lblLid">@Model.BankName</label></td>
                                </tr>
                                @*<tr>
                                    <th>Address</th>

                                    <td>:&nbsp;<span id="rptShipTo_lblAddress2_0">@Model.Address</span></td>
                                </tr>*@


                                <tr></tr>



                            </tbody>
                        </table>


                    </div>

                </div>

                <div class="row-fluid">

                    <div class="span12" style="min-height: 400px !important;">
                        <table class="table inventory" style="border-bottom: 1px solid ;" cellspacing="0" cellpadding="0">
                            <thead>
                                <tr style="  background: #f1f1f1;border-right: 1px solid #000;border-bottom: 1px solid #000;	">
                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000"><span>S.No.</span></th>
                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000;width: 215px;"><span>Product Name</span></th>

                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000"><span>Qty.</span></th>

                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000"><span>Rate(₹)</span></th>
                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000">
                                        <span>BV</span><br>
                                    </th>
                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000; border-right:0px;">
                                        <table style="width: 100%;" cellspacing="0" cellpadding="0">
                                            <tbody>
                                                <tr>
                                                    <th colspan="2" style="border-bottom:1px solid #000;"><span>SGST</span></th>
                                                    <th colspan="2" style="border-bottom:1px solid #000;"><span>CGST</span></th>
                                                    <th colspan="2" style="border-bottom:1px solid #000;"><span>IGST</span></th>
                                                </tr>
                                                <tr>
                                                    <th style="width:15px;text-align:center;" data-rel="tooltips" title="Rate(%)"><small style="font-size:8px;">(%)</small></th>
                                                    <th style="width:40px;text-align:right;padding-right:2px;"><small style="font-size:8px;">Amt</small></th>
                                                    <th style="width:15px;text-align:center;" data-rel="tooltips" title="Rate(%)"><small style="font-size:8px;">(%)</small></th>
                                                    <th style="width:40px;text-align:right;padding-right:2px;"><small style="font-size:8px;">Amt</small></th>
                                                    <th style="width:15px;text-align:center;" data-rel="tooltips" title="Rate(%)"><small style="font-size:8px;">(%)</small></th>
                                                    <th style="width:40px;text-align:right;padding-right:2px;"><small style="font-size:8px;">Amt</small></th>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </th>
                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000"><small>Taxable <br> Amount(₹)</small></th>

                                    <th style="border-top: none; padding: 0px;border-bottom: 1px solid #000; border-right: none;"><span>SubTotal(₹)</span></th>
                                </tr>
                            </thead>

                            <tbody style="min-height:400px">
                                @{
                                    int k = 0;
                                }
                                @foreach (var item in Model.MemberOrderDetailList)
                                {
                                    k = k + 1;

                                    <tr>
                                        <td><span>@k</span></td>
                                        <td style="text-align: center;"><span class="product-name"> @item.ProductName @*Ayush Kwath Syrup1000ML*@</span></td>

                                        <td style="text-align: center;"><span class="salqty"> @item.Quantity@*1*@</span></td>

                                        <td style="text-align: center;"><span class="prorate">@item.PrdAmt@*1280*@</span></td>

                                        <td style="text-align: center;">
                                            <span class="bvline">
                                                @*@item.BV*@   108
                                            </span>
                                        </td>

                                        <td style="padding: 0px; border-right:0px;">
                                            <table style="width: 100%;" cellspacing="0" cellpadding="0" border="0">
                                                <tbody>
                                                    <tr>
                                                        <td style="width:15px;text-align:center;font-size:10px;"> @item.SGST@*6*@</td>
                                                        <td style="width:40px;padding-right:2px;text-align:right;"><small style="font-size:10px;">@item.SGSTAmt@*69*@</small></td>
                                                        <td style="width:15px;width:15px;text-align:center;"><small style="font-size:10px;">@item.CGST@*6*@</small></td>
                                                        <td style="width:40px;padding-right:2px; text-align:right;"><small style="font-size:10px;">@item.CGSTAmt@*69*@</small></td>
                                                        <td style="width:15px;text-align:center;"><small style="font-size:10px;"> @item.IGST</small></td>
                                                        <td style="width:40px;padding-right:2px;text-align:right;"><small style="font-size:10px;">₹@item.IGSTAmt</small></td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </td>
                                        <td style="text-align: center;"><span class="tax-amount">@item.TaxableAmt</span></td>
                                        <td style="text-align: center; border-right: none;"><span><span class="prototal"> @item.TotalAmt</span></span></td>

                                    </tr>
                                }
                            </tbody>



                        </table>

                    </div>
                </div>
                <div class="row-fluid">
                    <div class="span12">
                        <table class="table" cellspacing="0" cellpadding="0" style="min-height:20px; border-top: none;background: #f1f1f1;border-right: 1px solid #000;border-bottom: 1px solid #000;text-align: left;">

                            <tbody>
                                <tr style="">
                                    <td style="border-top: none; width:300px !important;border-right: 1px solid;">Total</td>

                                    <td style="border-top: none; text-align: center;width:30px !important;border-right: 1px solid;padding-right:1px;"><strong>@Model.MemberOrderDetailList.Sum(i => i.Quantity) @*@Model.TotalQnty*@</strong> </td>
                                    <td style="border-top: none;text-align: center;width:75px !important;border-right: 1px solid;padding-right:1px;"><strong>@Model.MemberOrderDetailList.Sum(i => i.PrdAmt)  @*@Model.TotalRate*@ </strong></td>

                                    <td style="border-top: none; width:50px !important;border-right: 1px solid;padding-right:1px;">
                                        <span id="bvTotal">0.00</span>
                                    </td>

                                    <td style="text-align: center; font-weight: 700; border-top: none;">
                                        <table style="width: 100%;min-height:20px;" cellspacing="0" cellpadding="0">
                                            <tbody>
                                                <tr>
                                                    <th style="border-top: none;text-align: right;width:50px!important;border-right: 1px solid;padding-right:1px;">
                                                        <div style="width:50px;">
                                                            @*@Model.TotalSGST*@ @Model.MemberOrderDetailList.Sum(i => i.SGSTAmt)
                                                        </div>
                                                    </th>
                                                    <th style="border-top: none;text-align: right;border-right: none;width:50px !important;padding-right:1px;border-right: 1px solid;">
                                                        <div class=" " style="border-right: none;width:50px;">
                                                            @*@Model.TotalCGST*@ @Model.MemberOrderDetailList.Sum(i => i.CGSTAmt)
                                                        </div>
                                                    </th>
                                                    <th class="igst-display" style="border-right: none;width:100px !important;border-right: 1px solid;padding-right:1px; border-top: none;text-align: right;">
                                                        <div class=" " style="width:100px;">
                                                            @Model.MemberOrderDetailList.Sum(i => i.IGSTAmt)
                                                        </div>
                                                    </th>
                                                </tr>

                                            </tbody>
                                        </table>

                                    </td>
                                    <td style="text-align: center; border-top: none;width:55px !important;border-right: 1px solid;padding-right:1px;"><strong class="taxtotal"> @Model.MemberOrderDetailList.Sum(i => i.TaxableAmt)</strong></td>
                                    <td style="text-align: center; border-right: none; border-top: none;width:75px !important;padding-right:1px;">
                                        <strong class="producttotal">@Model.MemberOrderDetailList.Sum(i => i.TotalAmt)  @*@Model.SubTotalFinal*@</strong>
                                    </td>
                                </tr>
                                <tr style="">
                                    <td colspan="6" style="border-top: none; width:300px !important;border-right: 1px solid;border-top: 1px solid; text-align:right">CashBack Amount</td>
                                    <td style="text-align: center; border-right: none; border-top: 1px solid;width:75px !important;padding-right:1px;">
                                        <strong class="producttotal">00  @*@Model.CashBackAmount*@</strong>
                                    </td>
                                </tr>
                                <tr style="">
                                    <td colspan="6" style="border-top: none; width:300px !important;border-right: 1px solid;border-top: 1px solid; text-align:right">Net Amount</td>
                                    <td style="text-align: center; border-right: none; border-top: 1px solid;width:75px !important;padding-right:1px;">
                                        <strong class="producttotal">  ₹@Model.PaidAmt.ToString()</strong>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="row-fluid" style="border-bottom: 1px solid;">
                    <div class="span12">
                        <table class="table" cellspacing="0" cellpadding="0">

                            <tbody>

                                <tr style="">
                                    <th style="width: 60% !important; border-right: 1px solid; border-top: 1px solid;">TERM &amp; CONDITION OF SALE</th>
                                    <th style="border-top: 1px solid; font-weight: 800;">FamilyN</th>
                                </tr>
                                <tr>
                                    <td style="width: 60% !important; border-right: 1px solid; border-top: 1px solid;">
                                        <p>
                                            1) Goods once sold will not be taken back.<br>
                                            2) Disputes if any are subject to Lucknow Jurisdiction only.<br>
                                            3) Interest   3% per month, compounded monthly shall be charged from Invoice date, If  payment is not received in full within 30 days from the invoice date.<br>
                                            4) All claims regarding this invoice must be presented within 3 days from the date of the delivery of goods.<br>
                                        </p>
                                    </td>
                                    <td style="border-top: 1px solid;">
                                        <div>
                                            <br>
                                            <br>
                                            <br>
                                            <br>
                                        </div>

                                        <table cellspacing="0" cellpadding="0">
                                            <tbody>
                                                <tr>


                                                    <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>
                                                    <th>Authorised Signatory</th>
                                                </tr>

                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="3" style="padding: 0px;">
                                        <table class="table" style="width: 100%; border: 1px solid #000">
                                            <tbody>
                                                <tr style="text-transform: capitalize;">

                                                    <td style="width: 50% !important; border-top: none;">
                                                        COMPANY PAN NO.: AADCL5023E
                                                    </td>
                                                    <td style="text-align:right;">
                                                        <span id="lblEmployeeID">Created By:FamilyN</span>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="3" style="text-align: center;">
                                        Corporate Office: Plot No. 6 Shiv Nagar, Sitapur Road,Lucknow - 226021
                                    </td>
                                </tr>
                            </tbody>
                        </table>

                    </div>
                </div>
            </div>
            <br>
            <input type="button" value="Print" onclick="printDiv()" style="text-align: center;margin-left: 337px;font-size: 17px;background: aliceblue;padding: 11px;">

        </div>
    </div>

    <script src="~/Content/plugins/jQuery/jQuery-2.1.4.min.js"></script>
    <script src="~/Print Invoice_files/jquery.min.js.download"></script>
</body>

<script>

    function printDiv() {
        $("#divcontent").show();
        window.print();

    }
</script>